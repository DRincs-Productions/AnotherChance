# game/1000_base_game/game/characters/flora/season_1/q_flora_cooking_chilli.rpy:175 #-#-# translate crowdin flora_quest_flora_cooking_chilli_give_pot_22875b57:
msgid "[_flora_s_neutral_] Yep."
msgstr "[_flora_s_neutral_] Так."

# game/1000_base_game/game/characters/flora/season_1/q_flora_cooking_chilli.rpy:176 #-#-# translate crowdin flora_quest_flora_cooking_chilli_give_pot_50898a7d:
