# game/1000_base_game/game/characters/flora/season_1/q_flora_cooking_chilli.rpy:150 #-#-# translate crowdin flora_quest_flora_cooking_chilli_give_pot_4c02e1d7:
msgid "[_mc_] Don't talk to me like that."
msgstr "[_mc_] Не розмовляй зі мною так."

# game/1000_base_game/game/characters/flora/season_1/q_flora_cooking_chilli.rpy:152 #-#-# translate crowdin flora_quest_flora_cooking_chilli_give_pot_ff5ff03b:
