# TODO: Translation updated at 2023-11-05 22:06 #|#|# # game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:47 #-#-# translate crowdin quest_isabelle_piano_start_af63ac0e:
msgid "[_isabelle_s_excited_] I had no idea you play the piano!"
msgstr "[_isabelle_s_excited_] Je n'avais pas idée que tu jouais du piano !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:50 #-#-# translate crowdin quest_isabelle_piano_start_11fdb519:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:53 #-#-# translate crowdin quest_isabelle_piano_start_8605f1d8:
msgid "[_mc_] I started recently."
msgstr "[_mc_] J'ai commencé récemment."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:54 #-#-# translate crowdin quest_isabelle_piano_start_c3c50c77:
msgid "[_isabelle_s_excited_] You're definitely getting good!"
msgstr "[_isabelle_s_excited_] Vous allez vraiment bien !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:55 #-#-# translate crowdin quest_isabelle_piano_start_2f75f70a:
msgid "[_mc_] Thank you! I feel like I'm finally starting to get the hang of it..."
msgstr "[_mc_] Merci ! J'ai l'impression que je commence enfin à avoir la pendre ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:56 #-#-# translate crowdin quest_isabelle_piano_start_2fa70d4c:
msgid "[_isabelle_s_neutral_] Maybe this is your true calling?"
msgstr "[_isabelle_s_neutral_] Peut-être que c'est votre vrai appel?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:57 #-#-# translate crowdin quest_isabelle_piano_start_08b7313a:
msgid "[_mc_] I don't know about that, but I do enjoy it."
msgstr "[_mc_] Je ne sais pas à ce sujet, mais j'en profite."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:58 #-#-# translate crowdin quest_isabelle_piano_start_4c331e63:
msgid "[_mc_] What is your favorite classical piece?"
msgstr "[_mc_] Quel est votre morceau classique préféré?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:59 #-#-# translate crowdin quest_isabelle_piano_start_85dac08a:
msgid "[_isabelle_s_concerned_left_] It's hard to name just one... but I think the nocturnes are magical."
msgstr "[_isabelle_s_concerned_left_] C'est difficile de ne citer qu'un seul ... mais je pense que les nocturnes sont magiques."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:60 #-#-# translate crowdin quest_isabelle_piano_start_93a46ee5:
msgid "[_mc_] Didn't take you for a Chopin-chick."
msgstr "[_mc_] Ne vous a pas pris pour un Chopin-chick."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:61 #-#-# translate crowdin quest_isabelle_piano_start_ca415427:
msgid "[_isabelle_s_skeptical_] What did you think?"
msgstr "[_isabelle_s_skeptical_] Qu'avez-vous pensé?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:62 #-#-# translate crowdin quest_isabelle_piano_start_bcb76fa3:
msgid "[_mc_] Maybe a Beethoven-broad?"
msgstr "[_mc_] Peut-être un Beethoven-large?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:63 #-#-# translate crowdin quest_isabelle_piano_start_9f3bad76:
msgid "[_isabelle_s_confident_] And what do you call a girl who is into Mozart?"
msgstr "[_isabelle_s_confident_] Et qu'appelez vous une fille qui est dans Mozart?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:64 #-#-# translate crowdin quest_isabelle_piano_start_2eff993b:
msgid "[_mc_] Don't be ridiculous. Girls don't like Mozart. "
msgstr "[_mc_] Ne pas être ridicule. Les filles n'aiment pas Mozart. "

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:66 #-#-# translate crowdin quest_isabelle_piano_start_9efbeecc:
msgid "[_isabelle_s_laughing_] Hah! You're funny!"
msgstr "[_isabelle_s_laughing_] Hah ! Tu es drôle !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:67 #-#-# translate crowdin quest_isabelle_piano_start_dd0b0c31:
msgid "[_mc_] Thanks! You're not so bad yourself."
msgstr "[_mc_] Merci ! Tu n'es pas si mal toi-même."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:71 #-#-# translate crowdin quest_isabelle_piano_start_4d2b0969:
msgid "[_mc_] It comes naturally to me... I'm very good with my fingers."
msgstr "[_mc_] C'est naturellement pour moi ... Je suis très bien avec mes doigts."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:72 #-#-# translate crowdin quest_isabelle_piano_start_eff078d2:
msgid "[_isabelle_s_neutral_] Is that right?"
msgstr "[_isabelle_s_neutral_] Est-ce bien?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:73 #-#-# translate crowdin quest_isabelle_piano_start_810e06cc:
msgid "[_mc_] If you're lucky, I might show you sometime."
msgstr "[_mc_] Si vous avez de la chance, je pourrais vous montrer quelque temps."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:74 #-#-# translate crowdin quest_isabelle_piano_start_b75478e5:
msgid "[_isabelle_s_concerned_] I thought that's what you just did?"
msgstr "[_isabelle_s_concerned_] Je pensais que c'est ce que vous venez de faire?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:75 #-#-# translate crowdin quest_isabelle_piano_start_e8d204f0:
msgid "[_mc_] You haven't seen half of it yet..."
msgstr "[_mc_] Vous n'en avez pas encore vu la moitié ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:76 #-#-# translate crowdin quest_isabelle_piano_start_63b66a44:
msgid "[_mc_] I'm very experienced at pushing the right buttons and keys."
msgstr "[_mc_] Je suis très expérimenté à pousser les boutons et les touches de droite."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:77 #-#-# translate crowdin quest_isabelle_piano_start_56078d84:
msgid "[_isabelle_s_skeptical_] Relax, Mozart. Women are nothing like keyboards."
msgstr "[_isabelle_s_skeptical_] Relax, Mozart. Les femmes ne sont rien comme des claviers."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:78 #-#-# translate crowdin quest_isabelle_piano_start_2267a05d:
msgid "[_mc_] Women? I don't know what you're inferring."
msgstr "[_mc_] Femmes? Je ne sais pas ce que tu en dédues."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:79 #-#-# translate crowdin quest_isabelle_piano_start_165489b7:
msgid "[_isabelle_s_skeptical_] Sure, you don't..."
msgstr "[_isabelle_s_skeptical_] Bien sûr, vous ne ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:82 #-#-# translate crowdin quest_isabelle_piano_start_668854c2:
msgid "[_mc_] I had no idea you like music!"
msgstr "[_mc_] Je n'avais aucune idée que vous aimez la musique !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:83 #-#-# translate crowdin quest_isabelle_piano_start_6678b82a:
msgid "[_isabelle_s_skeptical_] That's a bit stand-offish, don't you think?"
msgstr "[_isabelle_s_skeptical_] C'est un peu choquante, ne pensez-vous pas?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:84 #-#-# translate crowdin quest_isabelle_piano_start_3b40fe14:
msgid "[_isabelle_s_skeptical_] Who doesn't like music?"
msgstr "[_isabelle_s_skeptical_] Qui n'aime pas la musique?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:87 #-#-# translate crowdin quest_isabelle_piano_start_11fdb519_1:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:92 #-#-# translate crowdin quest_isabelle_piano_start_97271d70:
msgid "[_mc_] Rap fans."
msgstr "[_mc_] fans de rap."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:93 #-#-# translate crowdin quest_isabelle_piano_start_97711efc:
msgid "[_mc_] Talking to a beat isn't music."
msgstr "[_mc_] Parler à un battement n'est pas de la musique."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:94 #-#-# translate crowdin quest_isabelle_piano_start_0c725dbf:
msgid "[_isabelle_s_confident_] What would you call it, then?"
msgstr "[_isabelle_s_confident_] Que appelez-vous, alors?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:95 #-#-# translate crowdin quest_isabelle_piano_start_aea10f26:
msgid "[_mc_] Rappers are basically poets with a bad attitude."
msgstr "[_mc_] Les rappers sont essentiellement des poètes avec une mauvaise attitude."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:96 #-#-# translate crowdin quest_isabelle_piano_start_3f843b69:
msgid "[_isabelle_s_laughing_] That is somehow accurate!"
msgstr "[_isabelle_s_laughing_] C'est exact !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:97 #-#-# translate crowdin quest_isabelle_piano_start_81907a89:
msgid "[_mc_] I always imagine Shakespeare with a ton of bling, rapping about quids, tobacco, and Lady Macbeth's booty."
msgstr "[_mc_] J'imagine toujours Shakespeare avec une tonne de bling, un rap sur les quids, le tabac et le butin de Lady Macbeth."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:98 #-#-# translate crowdin quest_isabelle_piano_start_86df3899:
msgid "[_isabelle_s_confident_] I'd love to see that!"
msgstr "[_isabelle_s_confident_] J'aimerais voir ça !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:101 #-#-# translate crowdin quest_isabelle_piano_start_809439a0:
msgid "[_mc_] Radio pop fans."
msgstr "[_mc_] Les amateurs de radio pop."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:103 #-#-# translate crowdin quest_isabelle_piano_start_a2b9531c:
msgid "[_isabelle_s_blush_] I suppose that's fair!"
msgstr "[_isabelle_s_blush_] Je suppose que c'est juste !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:104 #-#-# translate crowdin quest_isabelle_piano_start_8e264389:
msgid "[_isabelle_s_annoyed_] It represents everything I'm against."
msgstr "[_isabelle_s_annoyed_] Il représente tout ce que je suis contre."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:105 #-#-# translate crowdin quest_isabelle_piano_start_d85a3ac5:
msgid "[_mc_] Which is?"
msgstr "[_mc_] Qu'est-ce que c'est?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:106 #-#-# translate crowdin quest_isabelle_piano_start_a33352c6:
msgid "[_isabelle_s_angry_] The corporate music industry. The sexualization of women. The death of creativity."
msgstr "[_isabelle_s_angry_] L'industrie de la musique d'entreprise. La sexualisation des femmes. La mort de la créativité."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:107 #-#-# translate crowdin quest_isabelle_piano_start_09c19f0c:
msgid "[_isabelle_s_angry_] Art shouldn't be factory-made."
msgstr "[_isabelle_s_angry_] L'art ne doit pas être fabriqué en usine."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:108 #-#-# translate crowdin quest_isabelle_piano_start_66a31c7b:
msgid "[_mc_] I agree. At least about the last part."
msgstr "[_mc_] Je suis d'accord. Au moins de la dernière partie."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:109 #-#-# translate crowdin quest_isabelle_piano_start_966bba2c:
msgid "[_isabelle_s_annoyed_] So, you think the sexualization of women is fine?"
msgstr "[_isabelle_s_annoyed_] Alors, vous pensez que la sexualisation des femmes va bien?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:110 #-#-# translate crowdin quest_isabelle_piano_start_f672dfc9:
msgid "[_mc_] I plead the 5th."
msgstr "[_mc_] Je plaide pour le 5ème."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:114 #-#-# translate crowdin quest_isabelle_piano_start_ff4a42ac:
msgid "[_mc_] Bagpipe fans."
msgstr "[_mc_] éventails de Bagpipe."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:116 #-#-# translate crowdin quest_isabelle_piano_start_4fde6069:
msgid "[_isabelle_s_angry_] How very dare you?!"
msgstr "[_isabelle_s_angry_] Comment osez-vous?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:117 #-#-# translate crowdin quest_isabelle_piano_start_7df09d63:
msgid "[_mc_] Yeah, I said it!"
msgstr "[_mc_] Oui, je l'ai dit !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:118 #-#-# translate crowdin quest_isabelle_piano_start_5927af7e:
msgid "[_isabelle_s_angry_] That's... ugh!"
msgstr "[_isabelle_s_angry_] C'est. .. ugh !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:119 #-#-# translate crowdin quest_isabelle_piano_start_97447eaa:
msgid "[_isabelle_s_sad_] What is that that you don't like about it?"
msgstr "[_isabelle_s_sad_] Qu'est-ce que vous n'aimez pas à ce sujet?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:120 #-#-# translate crowdin quest_isabelle_piano_start_c9119c95:
msgid "[_mc_] It's loud and obnoxious."
msgstr "[_mc_] C'est bruyant et odieux."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:121 #-#-# translate crowdin quest_isabelle_piano_start_ff91e13d:
msgid "[_isabelle_s_laughing_] Shouldn't Americans love it, then?"
msgstr "[_isabelle_s_laughing_] Les Américains ne devraient-ils pas l'aimer?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:122 #-#-# translate crowdin quest_isabelle_piano_start_9891c5f1:
msgid "[_isabelle_s_laughing_] It's the sound of the Scottish highland! The sound of freedom!"
msgstr "[_isabelle_s_laughing_] C'est le son des hautes terres écossaises ! Le son de la liberté !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:123 #-#-# translate crowdin quest_isabelle_piano_start_30651fe3:
msgid "[_mc_] Why do you like Scotland so much? Doesn't the average English person dislike Scotland?"
msgstr "[_mc_] Pourquoi vous aimez tant l'Écosse? La personne anglaise moyenne n'aime pas l'Écosse?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:124 #-#-# translate crowdin quest_isabelle_piano_start_4ab464e8:
msgid "[_isabelle_s_confident_] I'm no stereotype... but also, I like what Scotland represents."
msgstr "[_isabelle_s_confident_] Je ne suis pas un stéréotype ... mais aussi, j'aime ce que représente l'Ecosse."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:125 #-#-# translate crowdin quest_isabelle_piano_start_c4b8024f:
msgid "[_mc_] I guess I can respect that."
msgstr "[_mc_] Je suppose que je peux le respecter."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:126 #-#-# translate crowdin quest_isabelle_piano_start_e65c88ae:
msgid "[_isabelle_s_neutral_] So, what are your thoughts on classical music in general?"
msgstr "[_isabelle_s_neutral_] Alors, quelles sont vos pensées sur la musique classique en général?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:127 #-#-# translate crowdin quest_isabelle_piano_start_8f8ac274:
msgid "[_mc_] I guess it's okay. I'm starting to appreciate it more and more."
msgstr "[_mc_] Je suppose que c'est bon. Je commence à l'apprécier de plus en plus."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:128 #-#-# translate crowdin quest_isabelle_piano_start_187a9b83:
msgid "[_isabelle_s_excited_] I've always been a fan! Nothing wrong with modern music, but there's something timeless about the classical pieces..."
msgstr "[_isabelle_s_excited_] J'ai toujours été fan ! Rien de mal avec la musique moderne, mais il y a quelque chose d'intemporel sur les morceaux classiques ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:129 #-#-# translate crowdin quest_isabelle_piano_start_05e58878:
msgid "[_isabelle_s_excited_] Would you mind playing another song for me?"
msgstr "[_isabelle_s_excited_] Est-ce que ça vous dérangerait de jouer une autre chanson pour moi?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:131 #-#-# translate crowdin quest_isabelle_piano_start_2d06ef0e:
msgid "This is going a bit off script, but perhaps this could be a good opportunity to earn her trust?"
msgstr "Cela va un peu en écriture, mais peut-être est-ce une bonne occasion de gagner sa confiance?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:132 #-#-# translate crowdin quest_isabelle_piano_start_c80415c0:
msgid "[kate] did ask me to get closer to [isabelle], after all."
msgstr "[kate] m'a demandé de me rapprocher du [isabelle], après tout."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:133 #-#-# translate crowdin quest_isabelle_piano_start_1b776282:
msgid "If only for the wrong reasons..."
msgstr "Si seulement pour les mauvaises raisons ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:134 #-#-# translate crowdin quest_isabelle_piano_start_eb8c16ff:
msgid "[_mc_] Sure, why not?"
msgstr "[_mc_] Sure, pourquoi pas?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:135 #-#-# translate crowdin quest_isabelle_piano_start_51ac7920:
msgid "[_mc_] What would you like to hear?"
msgstr "[_mc_] Qu'aimeriez-vous entendre?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:136 #-#-# translate crowdin quest_isabelle_piano_start_41302d46:
msgid "[_isabelle_s_blush_] Something by Chopin!"
msgstr "[_isabelle_s_blush_] Quelque chose de Chopin !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:137 #-#-# translate crowdin quest_isabelle_piano_start_7f6cffe0:
msgid "[_mc_] Hmm... I'd have to learn one..."
msgstr "[_mc_] Hmm ... Je devrais en apprendre un ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:138 #-#-# translate crowdin quest_isabelle_piano_start_cba0da8a:
msgid "[_isabelle_s_blush_] Would you do that for me?"
msgstr "[_isabelle_s_blush_] Est-ce que vous le feriez pour moi?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:139 #-#-# translate crowdin quest_isabelle_piano_start_d3d4b5ed:
msgid "[_mc_] I guess? It might take some time, though."
msgstr "[_mc_] Je suppose? Cela pourrait prendre un certain temps."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:140 #-#-# translate crowdin quest_isabelle_piano_start_d562b93b:
msgid "[_isabelle_s_confident_] That's fine!"
msgstr "[_isabelle_s_confident_] C'est bien !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:141 #-#-# translate crowdin quest_isabelle_piano_start_4ab83a33:
msgid "[_isabelle_s_confident_] If you do learn one, please let me know. I'd love to hear it."
msgstr "[_isabelle_s_confident_] Si vous en apprenez une, veuillez me le faire savoir. J'aimerais l'entendre."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:142 #-#-# translate crowdin quest_isabelle_piano_start_b9710c73:
msgid "[_mc_] Sure thing."
msgstr "[_mc_] Bien sûr."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:143 #-#-# translate crowdin quest_isabelle_piano_start_d20d2542:
msgid "[_isabelle_s_laughing_] I can't wait! Thanks for the tune, [mc]! Talk to you later!"
msgstr "[_isabelle_s_laughing_] Je ne peux pas attendre ! Merci pour la musique, [mc]! Parlez à vous plus tard !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:145 #-#-# translate crowdin quest_isabelle_piano_start_22fdebb8:
msgid "Chopin pieces aren't the easiest. Most of them are way above music class difficulty, so there probably aren't any scores in the classroom..."
msgstr "Les morceaux de Chopin ne sont pas les plus faciles. La plupart d'entre eux sont bien au-dessus de la difficulté des classes de musique, donc il n'y a probablement pas de scores dans la salle de classe ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_piano.rpy:146 #-#-# translate crowdin quest_isabelle_piano_start_8ce1a84d:
msgid "Luckily, there's always the internet and printers."
