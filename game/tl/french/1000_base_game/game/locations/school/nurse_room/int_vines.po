# game/1000_base_game/game/locations/school/nurse_room/int_vines.rpy:151 #-#-# translate crowdin school_nurse_room_sleepvine_use_item_595574f4:
msgid "Like a hissing cat, this vine went to find another pillow!"
msgstr "Comme un chat qui s'embrasse, cette vigne est allée trouver un autre oreiller !"

# game/1000_base_game/game/locations/school/nurse_room/int_vines.rpy:153 #-#-# translate crowdin school_nurse_room_sleepvine_use_item_7504dc23:
