# game/1000_base_game/game/characters/lindsey/season_2/q_lindsey_voluntary.rpy:906 #-#-# translate crowdin quest_lindsey_voluntary_delivery_a7b241ab:
msgid "I better get there fast so I don't end up having to clean toilets in the basement or something."
msgstr "Meglio arrivare lì veloce quindi non finisco per dover pulire i servizi igienici nel seminterrato o qualcosa."

# game/1000_base_game/game/characters/lindsey/season_2/q_lindsey_voluntary.rpy:907 #-#-# translate crowdin quest_lindsey_voluntary_delivery_b456aa47:
