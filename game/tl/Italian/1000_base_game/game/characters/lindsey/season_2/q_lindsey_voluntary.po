# game/1000_base_game/game/characters/lindsey/season_2/q_lindsey_voluntary.rpy:950 #-#-# translate crowdin quest_lindsey_voluntary_volunteer_71605ba6:
msgid "All right. Time to look presentable."
msgstr "Va bene. È tempo di sembrare presentabile."

# game/1000_base_game/game/characters/lindsey/season_2/q_lindsey_voluntary.rpy:951 #-#-# translate crowdin quest_lindsey_voluntary_volunteer_46a708ca:
