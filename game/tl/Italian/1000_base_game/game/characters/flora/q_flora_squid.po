# game/1000_base_game/game/characters/flora/q_flora_squid.rpy:32 #-#-# translate crowdin quest_flora_squid_start_fba28f80:
msgid "I'll take that as a yes."
msgstr "Lo prenderò come un sì."

# game/1000_base_game/game/characters/flora/q_flora_squid.rpy:34 #-#-# translate crowdin quest_flora_squid_start_c30c3264:
msgid "[_flora_] You left with her!"
