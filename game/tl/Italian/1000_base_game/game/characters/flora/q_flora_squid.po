# game/1000_base_game/game/characters/flora/q_flora_squid.rpy:24 #-#-# translate crowdin quest_flora_squid_start_54696dae:
msgid "[_mc_] What did I do?"
msgstr "[_mc_] Che cosa ho fatto?"

# game/1000_base_game/game/characters/flora/q_flora_squid.rpy:25 #-#-# translate crowdin quest_flora_squid_start_32dc149e:
msgid "[_flora_] You know what you did!"
msgstr "[_flora_] Sai cosa hai fatto!"

# game/1000_base_game/game/characters/flora/q_flora_squid.rpy:26 #-#-# translate crowdin quest_flora_squid_start_46a4d46b:
msgid "[_mc_] I really don't!"
msgstr "[_mc_] non lo faccio davvero!"

# game/1000_base_game/game/characters/flora/q_flora_squid.rpy:27 #-#-# translate crowdin quest_flora_squid_start_8e819ba5:
msgid "[_flora_] You made your choice! Don't talk to me!"
msgstr "[_flora_] Hai fatto la tua scelta! Non parlarmi!"

# game/1000_base_game/game/characters/flora/q_flora_squid.rpy:28 #-#-# translate crowdin quest_flora_squid_start_c0b6fbcf:
msgid "What the hell is she talking about?"
