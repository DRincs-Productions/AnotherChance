# game/1000_base_game/game/characters/flora/q_flora_squid.rpy:29 #-#-# translate crowdin quest_flora_squid_start_11bedf1c:
msgid "Hmm..."
msgstr "Hmm..."

# game/1000_base_game/game/characters/flora/q_flora_squid.rpy:30 #-#-# translate crowdin quest_flora_squid_start_79414fe6:
msgid "[_mc_] Is this about [jacklyn]?"
msgstr "[_mc_] È circa [jacklyn]?"

# game/1000_base_game/game/characters/flora/q_flora_squid.rpy:31 #-#-# translate crowdin quest_flora_squid_start_0bb15cb3:
msgid "[_flora_] ..."
