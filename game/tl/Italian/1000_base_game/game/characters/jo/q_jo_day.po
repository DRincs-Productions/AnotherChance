# game/1000_base_game/game/characters/jo/q_jo_day.rpy:75 #-#-# translate crowdin quest_jo_day_flora_c07b5279:
msgid "[_flora_s_eyeroll_] Stop smirking."
msgstr "[_flora_s_eyeroll_] Smettetela di sorpassare."

# game/1000_base_game/game/characters/jo/q_jo_day.rpy:76 #-#-# translate crowdin quest_jo_day_flora_b7ff6695:
msgid "[_mc_] I'm not smirking."
