# game/1000_base_game/game/characters/isabelle/q_isabelle_red.rpy:75 #-#-# translate crowdin quest_isabelle_red_start_9c4a8b94:
msgid "[_mc_] Do you only have one bucket?"
msgstr "[_mc_] Hai solo un secchio?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_red.rpy:76 #-#-# translate crowdin quest_isabelle_red_start_869cddf0:
msgid "[_jacklyn_s_neutral_] Budget cuts whipped our asses."
msgstr "[_jacklyn_s_neutral_] I tagli di bilancio hanno montato i nostri asini."

# game/1000_base_game/game/characters/isabelle/q_isabelle_red.rpy:77 #-#-# translate crowdin quest_isabelle_red_start_3515c03a:
msgid "[_mc_] Well, do you know who borrowed it?"
