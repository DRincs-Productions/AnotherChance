# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:994 #-#-# translate crowdin quest_isabelle_hurricane_call_maxine_2eb1c55a:
msgid "Very few people would ever try to go up against [maxine]."
msgstr "Pochissime persone avrebbero mai provato a salire contro [maxine]."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:995 #-#-# translate crowdin quest_isabelle_hurricane_call_maxine_b1a72f6e:
