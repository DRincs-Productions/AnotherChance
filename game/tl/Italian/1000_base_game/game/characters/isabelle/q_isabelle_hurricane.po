# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:964 #-#-# translate crowdin quest_isabelle_hurricane_call_maxine_883a3f3b:
msgid "[_isabelle_s_annoyed_left_] Well, I have a surprise for you, you miserable twat."
msgstr "[_isabelle_s_annoyed_left_] Beh, ho una sorpresa per te, tu gemello miserabile."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:971 #-#-# translate crowdin quest_isabelle_hurricane_call_maxine_2283acd4:
