# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:42 #-#-# translate crowdin quest_isabelle_locker_start_ff31f790:
msgid "[_mc_] It's a capital mistake to theorize before one has data..."
msgstr "[_mc_] È un errore di capitale teorizzare prima di avere dati..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:43 #-#-# translate crowdin quest_isabelle_locker_start_c6140401:
msgid "[_isabelle_s_flirty_] A direct quote? Color me impressed!"
msgstr "[_isabelle_s_flirty_] Un preventivo diretto? Colore me impressionato!"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:44 #-#-# translate crowdin quest_isabelle_locker_start_a462ae1a:
msgid "[_mc_] You'll be even more impressed when I solve this case for you."
msgstr "[_mc_] Sarai ancora più impressionato quando risolvo questo caso per te."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:45 #-#-# translate crowdin quest_isabelle_locker_start_0210fa34:
msgid "[_mc_] I'll let you know when I have a lead."
msgstr "[_mc_] ti farò sapere quando ho un comando."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:46 #-#-# translate crowdin quest_isabelle_locker_start_29db47d7:
msgid "[_isabelle_s_blush_] Thanks so much for helping me out. I do appreciate it."
msgstr "[_isabelle_s_blush_] Grazie mille per avermi aiutato fuori. Lo apprezzo."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:48 #-#-# translate crowdin quest_isabelle_locker_start_273ff07b:
msgid "Let's see here. What would Sherlock have done?"
msgstr "Vediamo qui. Cosa avrebbe fatto Sherlock?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:49 #-#-# translate crowdin quest_isabelle_locker_start_9c491626:
msgid "Gathered data... interrogations... yes, let's interrogate people."
msgstr "Dati raccolti... interrogatori... sì, interroghiamo la gente."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:52 #-#-# translate crowdin quest_isabelle_locker_start_3f03d602:
msgid "[_mc_] I might have an idea..."
