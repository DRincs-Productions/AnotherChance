# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:34 #-#-# translate crowdin quest_isabelle_locker_start_ac5f843b:
msgid "[_isabelle_s_confident_] And that's supposed to be you?"
msgstr "[_isabelle_s_confident_] E dovrebbe essere voi?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:35 #-#-# translate crowdin quest_isabelle_locker_start_3df01cd4:
msgid "[_mc_] This will be elementary, my dear [isabelle]."
