# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:29 #-#-# translate crowdin quest_isabelle_locker_start_685cf414:
msgid "[_mc_] You would've done the same for me."
msgstr "[_mc_] Tu avresti fatto lo stesso per me."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:30 #-#-# translate crowdin quest_isabelle_locker_start_0b9f251c:
msgid "[_isabelle_s_laughing_] Absolutely!"
msgstr "[_isabelle_s_laughing_] Assolutamente!"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:33 #-#-# translate crowdin quest_isabelle_locker_start_4329129d:
msgid "[_mc_] Leave this in the hands of Sherlock!"
