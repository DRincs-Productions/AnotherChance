# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:15 #-#-# translate crowdin quest_isabelle_locker_start_b43dbea3:
msgid "[_mc_] Hey, it's fine! Did you manage to get new books?"
msgstr "[_mc_] Hey, va bene! Sei riuscito a ottenere nuovi libri?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:16 #-#-# translate crowdin quest_isabelle_locker_start_f119e3e8:
msgid "[_isabelle_s_neutral_] Yeah, I did. So, at least my grades won't suffer from this..."
