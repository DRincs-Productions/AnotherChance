# TODO: Translation updated at 2023-11-05 22:06 #|#|# # game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:3 #-#-# translate crowdin isabelle_quest_stolen_start_d518eb58:
msgid "[_isabelle_s_angry_] Can you bloody believe it?!"
msgstr "[_isabelle_s_angry_] Riesci a crederci sanguinoso?!"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:4 #-#-# translate crowdin isabelle_quest_stolen_start_dc1e5fed:
msgid "[_isabelle_s_angry_] Someone broke into my locker!"
msgstr "[_isabelle_s_angry_] Qualcuno si è rotto nel mio armadio!"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:7 #-#-# translate crowdin isabelle_quest_stolen_start_11fdb519:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:10 #-#-# translate crowdin isabelle_quest_stolen_start_ec8d8b4b:
msgid "[_mc_] Was anything stolen?"
msgstr "[_mc_] È stato rubato qualcosa?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:11 #-#-# translate crowdin isabelle_quest_stolen_start_3301db0c:
msgid "[_isabelle_s_annoyed_] Well, yes!"
msgstr "[_isabelle_s_annoyed_] Beh, sì!"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:12 #-#-# translate crowdin isabelle_quest_stolen_start_be59fc22:
msgid "[_mc_] What was it?"
msgstr "[_mc_] Che cos'era?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:13 #-#-# translate crowdin isabelle_quest_stolen_start_737ad7aa:
msgid "[_isabelle_s_concerned_left_] That's unimportant."
msgstr "[_isabelle_s_concerned_left_] Non è importante."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:14 #-#-# translate crowdin isabelle_quest_stolen_start_ae1bfd5a:
msgid "[_isabelle_s_skeptical_] What matters is finding and bringing the culprit to justice!"
msgstr "[_isabelle_s_skeptical_] Ciò che conta è trovare e portare il colpevole alla giustizia!"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:15 #-#-# translate crowdin isabelle_quest_stolen_start_3da3682b:
msgid "That's an odd stance to take... you'd think that finding the stolen item would be her number one priority."
msgstr "Questa è una posizione strana da prendere... Pensi che trovare l'oggetto rubato sarebbe la sua priorità numero uno."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:16 #-#-# translate crowdin isabelle_quest_stolen_start_16e51f27:
msgid "[_mc_] So, what are you going to do about it?"
msgstr "[_mc_] Allora, che cosa farai?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:17 #-#-# translate crowdin isabelle_quest_stolen_start_df9770eb:
msgid "[_isabelle_s_thinking_] First we need suspects."
msgstr "[_isabelle_s_thinking_] Prima abbiamo bisogno di sospetti."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:18 #-#-# translate crowdin isabelle_quest_stolen_start_bb54adc5:
msgid "[_mc_] We?"
msgstr "[_mc_] Noi?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:19 #-#-# translate crowdin isabelle_quest_stolen_start_c3924613:
msgid "[_isabelle_s_cringe_] An injustice has been committed. Are you just planning to watch?"
msgstr "[_isabelle_s_cringe_] È stata commessa un'ingiustizia. Hai solo intenzione di guardare?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:20 #-#-# translate crowdin isabelle_quest_stolen_start_f9ff4b1f:
msgid "[_mc_] I mean, that was the first thought that popped into my head..."
msgstr "[_mc_] Voglio dire, che è stato il primo pensiero che spuntò nella mia testa..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:21 #-#-# translate crowdin isabelle_quest_stolen_start_bc176bfd:
msgid "[_isabelle_s_thinking_] Well, I'm going to do some investigating at least."
msgstr "[_isabelle_s_thinking_] Beh, farò almeno un po' di indagine."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:22 #-#-# translate crowdin isabelle_quest_stolen_start_af8c202a:
msgid "[_mc_] Do you even know where to start?"
msgstr "[_mc_] Sai anche da dove cominciare?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:23 #-#-# translate crowdin isabelle_quest_stolen_start_710f06cc:
msgid "[_isabelle_s_cringe_] Wouldn't surprise me if [kate]'s behind it."
msgstr "[_isabelle_s_cringe_] non mi sorprende se [kate]è alle spalle."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:24 #-#-# translate crowdin isabelle_quest_stolen_start_7967c5e6:
msgid "[_mc_] Really? When was the thing stolen?"
msgstr "[_mc_] Davvero? Quando è stata rubata la cosa?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:25 #-#-# translate crowdin isabelle_quest_stolen_start_08bfa59c:
msgid "[_isabelle_s_thinking_] Hmm... I reckon it was this morning during my time in the gym."
msgstr "[_isabelle_s_thinking_] Hmm... Ho pensato che questa mattina durante il mio tempo in palestra."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:26 #-#-# translate crowdin isabelle_quest_stolen_start_8587e4a7:
msgid "[_mc_] I was with [kate] then, so it definitely wasn't her."
msgstr "[_mc_] ero con [kate] allora, quindi sicuramente non era lei."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:27 #-#-# translate crowdin isabelle_quest_stolen_start_a51ff68b:
msgid "It feels a bit wrong to lie, but her throwing baseless accusations around is just as bad."
msgstr "Ci si sente un po' sbagliati a mentire, ma le sue accuse infondate sono altrettanto cattive."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:28 #-#-# translate crowdin isabelle_quest_stolen_start_4a077dd3:
msgid "[_isabelle_s_annoyed_] Damn, that's annoying... it totally felt like something she'd do."
msgstr "[_isabelle_s_annoyed_] Damn, è fastidioso... si sentiva totalmente come qualcosa che avrebbe fatto."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:29 #-#-# translate crowdin isabelle_quest_stolen_start_5f078177:
msgid "[_mc_] I don't know. [kate] is a lot of things... but a thief? Doesn't really sound like her."
msgstr "[_mc_] non lo so. [kate] è un sacco di cose... ma un ladro? Non sembra davvero come lei."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:30 #-#-# translate crowdin isabelle_quest_stolen_start_ae35a46d:
msgid "[_isabelle_s_annoyed_] What do you mean?"
msgstr "[_isabelle_s_annoyed_] Che cosa intendi?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:31 #-#-# translate crowdin isabelle_quest_stolen_start_a51f2433:
msgid "[_mc_] She's pretty loaded. What would she get out of stealing?"
msgstr "[_mc_] Lei è piuttosto caricata. Che cosa otterrebbe di rubare?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:32 #-#-# translate crowdin isabelle_quest_stolen_start_8dbf3d33:
msgid "[_isabelle_s_eyeroll_] Are you serious? It's obviously just a bullying tactic."
msgstr "[_isabelle_s_eyeroll_] Sei serio? È ovviamente solo una tattica di bullismo."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:33 #-#-# translate crowdin isabelle_quest_stolen_start_b9af881e:
msgid "[_mc_] Hey, no need to be rude."
msgstr "[_mc_] Hey, non c'è bisogno di essere maleducato."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:34 #-#-# translate crowdin isabelle_quest_stolen_start_a2801471:
msgid "[_isabelle_s_annoyed_] If you're being willfully ignorant of the motives, it's your own fault."
msgstr "[_isabelle_s_annoyed_] Se sei intenzionalmente ignorante delle motivazioni, è colpa tua."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:35 #-#-# translate crowdin isabelle_quest_stolen_start_6a75e61e:
msgid "[_isabelle_s_annoyed_left_] I'm going to have a word with her."
msgstr "[_isabelle_s_annoyed_left_] Avrò una parola con lei."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:37 #-#-# translate crowdin isabelle_quest_stolen_start_910703c6:
msgid "Ugh, this again? Why can't she just leave things be?"
msgstr "Ugh, ancora questo? Perché non può lasciare le cose soltanto?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:38 #-#-# translate crowdin isabelle_quest_stolen_start_2e788f45:
msgid "[kate] would probably appreciate it if I gave her a heads up."
msgstr "[kate] probabilmente apprezzerebbe se le dessi una testa in su."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:42 #-#-# translate crowdin isabelle_quest_stolen_start_ca223427:
msgid "[_mc_] Have you talked to the [guard]?"
msgstr "[_mc_] Hai parlato con la [guard]?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:43 #-#-# translate crowdin isabelle_quest_stolen_start_db107545:
msgid "[_isabelle_s_eyeroll_] He told me he couldn't do anything about it."
msgstr "[_isabelle_s_eyeroll_] Mi disse che non riusciva a fare nulla a riguardo."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:44 #-#-# translate crowdin isabelle_quest_stolen_start_830fa13f:
msgid "[_mc_] That sounds odd... how come?"
msgstr "[_mc_] Sembra strano... come?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:45 #-#-# translate crowdin isabelle_quest_stolen_start_c4892fe5:
msgid "Knowing him, probably laziness."
msgstr "Lo conosce, probabilmente pigrizia."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:46 #-#-# translate crowdin isabelle_quest_stolen_start_b47aefbd:
msgid "[_isabelle_s_annoyed_] Apparently, if the locker itself isn't broken, you have to take it up with the police."
msgstr "[_isabelle_s_annoyed_] A quanto pare, se l'armadietto stesso non è rotto, devi prenderlo con la polizia."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:47 #-#-# translate crowdin isabelle_quest_stolen_start_906500fe:
msgid "[_isabelle_s_annoyed_left_] He doesn't do investigations."
msgstr "[_isabelle_s_annoyed_left_] Lui non fa indagini."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:48 #-#-# translate crowdin isabelle_quest_stolen_start_b80beef5:
msgid "[_mc_] If the lock wasn't broken, are you sure you didn't just misplace it?"
msgstr "[_mc_] Se la serratura non è stata rotta, sei sicuro di non averla appena piazzata?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:49 #-#-# translate crowdin isabelle_quest_stolen_start_85f3e052:
msgid "[_isabelle_s_angry_] That's exactly what the [guard] said! And the answer is no. I wouldn't lose something like this."
msgstr "[_isabelle_s_angry_] È esattamente quello che l' [guard] ha detto! E la risposta è no. Non perderei qualcosa del genere."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:50 #-#-# translate crowdin isabelle_quest_stolen_start_7a082c95:
msgid "[_mc_] Well, someone must've seen something..."
msgstr "[_mc_] Beh, qualcuno deve aver visto qualcosa..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:53 #-#-# translate crowdin isabelle_quest_stolen_start_11fdb519_1:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:56 #-#-# translate crowdin isabelle_quest_stolen_start_977db975:
msgid "[_mc_] Maybe we can ask [maxine]? She's got this school under a microscope."
msgstr "[_mc_] Forse possiamo chiedere [maxine]? Ha questa scuola sotto un microscopio."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:60 #-#-# translate crowdin isabelle_quest_stolen_start_ce724cfc:
msgid "[_mc_] I'll do some snooping. You stay put."
msgstr "[_mc_] farò un po' di snooping. Rimani messo."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:61 #-#-# translate crowdin isabelle_quest_stolen_start_feff8eed:
msgid "[_isabelle_s_thinking_] I mean, that's nice and all, but I'd like to get involved..."
msgstr "[_isabelle_s_thinking_] Voglio dire, che è bello e tutto, ma vorrei essere coinvolto..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:62 #-#-# translate crowdin isabelle_quest_stolen_start_c6c3275f:
msgid "[_isabelle_s_thinking_] I'm the victim of this theft."
msgstr "[_isabelle_s_thinking_] Io sono la vittima di questo furto."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:63 #-#-# translate crowdin isabelle_quest_stolen_start_976fa6a6:
msgid "[_mc_] That's the point — you asking around might not work."
msgstr "[_mc_] Questo è il punto — si chiede intorno potrebbe non funzionare."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:64 #-#-# translate crowdin isabelle_quest_stolen_start_8f8e2042:
msgid "[_isabelle_s_cringe_] So, I'm supposed to just sit here?"
msgstr "[_isabelle_s_cringe_] Quindi, dovrei semplicemente sedermi qui?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:67 #-#-# translate crowdin isabelle_quest_stolen_start_11fdb519_2:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:70 #-#-# translate crowdin isabelle_quest_stolen_start_2093ed92:
msgid "[_mc_] Yes, sit pretty like a lady for once and let me do the work."
msgstr "[_mc_] Sì, siediti abbastanza come una signora per una volta e lasciami fare il lavoro."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:73 #-#-# translate crowdin isabelle_quest_stolen_start_3569d05f:
msgid "[_isabelle_s_angry_] That's the most repulsive thing I've heard all week!"
msgstr "[_isabelle_s_angry_] Questa è la cosa più ripugnante che abbia sentito per tutta la settimana!"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:74 #-#-# translate crowdin isabelle_quest_stolen_start_17c84cc1:
msgid "[_isabelle_s_angry_] I don't need your help with this. Good day."
msgstr "[_isabelle_s_angry_] Non ho bisogno del tuo aiuto con questo. Buon giorno."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:76 #-#-# translate crowdin isabelle_quest_stolen_start_7fa1125c:
msgid "Welp, what a bitch. Although, I am a little curious about the theft now..."
msgstr "Welp, che cagna. Anche se, sono un po 'curioso circa il furto ora..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:77 #-#-# translate crowdin isabelle_quest_stolen_start_1239f49a:
msgid "Maybe I'll do some investigating on my own."
msgstr "Forse farò un po' di investigazione per conto mio."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:78 #-#-# translate crowdin isabelle_quest_stolen_start_05cd6ce7:
msgid "The logical first step is obviously talking to the [guard]."
msgstr "Il primo passo logico è ovviamente parlare con la [guard]."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:79 #-#-# translate crowdin isabelle_quest_stolen_start_dc8d88fa:
msgid "It doesn't matter if [isabelle] already interrogated him in her preliminary investigation."
msgstr "Non importa se [isabelle] lo ha già interrogato nella sua indagine preliminare."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:80 #-#-# translate crowdin isabelle_quest_stolen_start_4d02614f:
msgid "After that, you start from scratch. Treat everyone as a liar."
msgstr "Dopo di che, si inizia da zero. Tratta tutti come un bugiardo."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:84 #-#-# translate crowdin isabelle_quest_stolen_start_5092f045:
msgid "[_mc_] No one ever notices me... I'm the perfect spy."
msgstr "[_mc_] Nessuno mi nota mai... Sono la spia perfetta."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:85 #-#-# translate crowdin isabelle_quest_stolen_start_a4dfdd74:
msgid "[_isabelle_s_afraid_] Don't say that!"
msgstr "[_isabelle_s_afraid_] Non dirlo!"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:86 #-#-# translate crowdin isabelle_quest_stolen_start_6ab1feb9:
msgid "[_mc_] Well, it's true."
msgstr "[_mc_] Beh, è vero."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:87 #-#-# translate crowdin isabelle_quest_stolen_start_9158572c:
msgid "[_isabelle_s_cringe_] It's not! Else I wouldn't be talking to you now."
msgstr "[_isabelle_s_cringe_] Non è così! Altrimenti non ti parlerei ora."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:88 #-#-# translate crowdin isabelle_quest_stolen_start_2beece36:
msgid "[_mc_] Well, you're different."
msgstr "[_mc_] Beh, sei diverso."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:89 #-#-# translate crowdin isabelle_quest_stolen_start_1dce5c7b:
msgid "[_isabelle_s_thinking_] Different?"
msgstr "[_isabelle_s_thinking_] Diverso?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:90 #-#-# translate crowdin isabelle_quest_stolen_start_37500ec3:
msgid "[_mc_] In a good way... you stand out from the crowd."
msgstr "[_mc_] In un buon modo... ti distingui dalla folla."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:92 #-#-# translate crowdin isabelle_quest_stolen_start_887a8c96:
msgid "[_isabelle_s_flirty_] Thanks, I think?"
msgstr "[_isabelle_s_flirty_] Grazie, penso?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:93 #-#-# translate crowdin isabelle_quest_stolen_start_1993eb0e:
msgid "That felt clumsy, but [isabelle] seemed to like it nonetheless."
msgstr "Che si sentiva goffo, ma [isabelle] sembrava comunque piacere."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:94 #-#-# translate crowdin isabelle_quest_stolen_start_8152e1b3:
msgid "[_isabelle_s_flirty_] I guess I can let you do some snooping for me..."
msgstr "[_isabelle_s_flirty_] Immagino che posso farti fare qualche spuntino per me..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:95 #-#-# translate crowdin isabelle_quest_stolen_start_1dbc46d7:
msgid "[_mc_] Okay, starting by asking [maxine] is our best bet. She even calls her cat \"Detective [spinach].\""
msgstr "[_mc_] Okay, iniziando a chiedere [maxine] è la nostra migliore scommessa. Chiama anche il suo gatto \"Detective [spinach].\""

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:99 #-#-# translate crowdin isabelle_quest_stolen_start_fadcdf3b:
msgid "[_mc_] Do you think this is retaliation?"
msgstr "[_mc_] Pensi che questa sia ritorsione?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:100 #-#-# translate crowdin isabelle_quest_stolen_start_7a8d768d:
msgid "[_isabelle_s_skeptical_] From [kate]?"
msgstr "[_isabelle_s_skeptical_] From [kate]?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:101 #-#-# translate crowdin isabelle_quest_stolen_start_3c25bc6f:
msgid "[_mc_] Wouldn't surprise me..."
msgstr "[_mc_] Non mi sorprende..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:102 #-#-# translate crowdin isabelle_quest_stolen_start_43c0a89a:
msgid "[_isabelle_s_skeptical_] Would she really stoop that low?"
msgstr "[_isabelle_s_skeptical_] avrebbe davvero capovolgere quel basso?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:103 #-#-# translate crowdin isabelle_quest_stolen_start_d9ac1018:
msgid "[_mc_] You have no idea."
msgstr "[_mc_] Non hai idea."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:104 #-#-# translate crowdin isabelle_quest_stolen_start_92b514f3:
msgid "[_isabelle_s_concerned_] You're right, I don't."
msgstr "[_isabelle_s_concerned_] Hai ragione, non lo so."

# game/1000_base_game/game/characters/isabelle/q_isabelle_stolen.rpy:105 #-#-# translate crowdin isabelle_quest_stolen_start_28d31fda:
msgid "[_isabelle_s_concerned_left_] ...but she does seem like a good suspect."
msgstr "[_isabelle_s_concerned_left_] ...ma lei sembra un buon sospetto."

