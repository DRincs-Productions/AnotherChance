msgstr "[_mc_] L'unico che mi ha dato una possibilità quando nessun altro lo farebbe."

# game/1000_base_game/game/characters/isabelle/season_2/q_isabelle_gesture.rpy:575 #-#-# translate crowdin quest_isabelle_gesture_apology_073eb2a2:
msgid "[_mc_] And the only one that makes me happy."
msgstr "[_mc_] E l'unico che mi rende felice."

# game/1000_base_game/game/characters/isabelle/season_2/q_isabelle_gesture.rpy:576 #-#-# translate crowdin quest_isabelle_gesture_apology_7efeecf5:
msgid "[_isabelle_s_blush_] Truly? Do you mean it?"
msgstr "[_isabelle_s_blush_] Veramente? Lo intendi?"

# game/1000_base_game/game/characters/isabelle/season_2/q_isabelle_gesture.rpy:577 #-#-# translate crowdin quest_isabelle_gesture_apology_5f457fe5:
