# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:345 #-#-# translate crowdin quest_nurse_venting_ready_429a1969:
msgid "[_nurse_s_thinking_] Strip?"
msgstr "[_nurse_s_thinking_] Striscia?"

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:346 #-#-# translate crowdin quest_nurse_venting_ready_60fa5763:
