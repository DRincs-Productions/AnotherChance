msgstr "[_nurse_] Okay, giusto."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:599 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_45b3f836:
msgid "[_mc_] What are you hearing now?"
msgstr "[_mc_] Cosa stai sentendo adesso?"

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:600 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_f3df12e8:
msgid "[_nurse_] You!"
msgstr "[_nurse_] Tu!"

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:601 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_3664961a:
msgid "[_mc_] Yes, but put the walkie-talkie\n"
"down and then listen."
msgstr "[_mc_] Sì, ma metti giù il walkie-talkie\n"
"e poi ascolta."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:602 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_f5ef45eb:
msgid "[_nurse_] I did! I heard you\n"
"through the wall!"
msgstr "[_nurse_] l'ho fatto! Ti ho sentito\n"
"attraverso il muro!"

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:603 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_2b35b6fe:
msgid "[_mc_] ..."
