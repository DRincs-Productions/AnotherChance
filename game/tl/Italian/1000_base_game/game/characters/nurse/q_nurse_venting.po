# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:604 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_dd847a7b_6:
msgid "[_nurse_] ..."
msgstr "[_nurse_] ..."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:605 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_21c0f0bd:
msgid "[_mc_] Did you just go\n"
"in a circle?"
msgstr "[_mc_] Sei appena andato\n"
"in cerchio?"

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:606 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_3a2b7f85:
msgid "[_nurse_] Possibly!"
msgstr "[_nurse_] Possibilmente!"

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:614 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_65dd1493:
msgid "[_nurse_] It's dark in here. I keep catching\n"
"on the vent's edges."
msgstr "[_nurse_] È buio dentro qui. Continuo a catturare\n"
"sui bordi della bocca."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:615 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_622ad2ba:
msgid "[_mc_]  But are you making progress?\n"
"Can you see where you're going?"
msgstr "[_mc_]  Ma stai facendo progressi?\n"
"Riesci a vedere dove stai andando?"

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:616 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_f4ae5ad0:
msgid "[_nurse_] Sort of... there's openings\n"
"that let in some light..."
msgstr "[_nurse_] Ordina per... ci sono le aperture\n"
"che lasciano un po' di luce..."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:617 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_40de629d:
msgid "[_nurse_] And it's so hot. I'm sweating.\n"
"The dust is sticking to my sweat."
msgstr "[_nurse_] Ed è così caldo. Sto sudando.\n"
"La polvere si attacca al mio sudore."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:618 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_25ecbe8b:
msgid "[_mc_] We'll get you showered\n"
"off once you're done."
msgstr "[_mc_] Ti faremo svuotare\n"
"una volta finito."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:619 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_39ccff21:
msgid "[_mc_] Just keep moving for now.\n"
"Have you gone up a floor yet?"
msgstr "[_mc_] Continua a muoverti per ora.\n"
"Sei già salito un piano?"

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:620 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_44b58b9a:
msgid "[_nurse_] Yes, but I can't find another\n"
"way up. I've passed a few\n"
"junctions and I may have\n"
"gotten turned around..."
msgstr "[_nurse_] Sì, ma non riesco a trovare un altro modo\n"
". Ho superato alcuni incroci\n"
"e potrei avere\n"
"girato intorno..."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:623 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_702b3ae7:
msgid "I shouldn't be surprised. She could get lost on a one-way street."
msgstr "Non dovrei essere sorpreso. Potrebbe perdersi in una strada a senso unico."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:626 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_6307b445:
msgid "[_mc_] Just keep moving. I'm sure\n"
"you'll find the way eventually."
msgstr "[_mc_] Basta continuare a muoversi. Sono sicuro che\n"
"troverai la strada alla fine."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:629 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_93e9f412_1:
msgid "[_nurse_] I have no idea where I am..."
msgstr "[_nurse_] Non ho idea di dove sono..."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:630 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_5f9c1c2e_1:
msgid "[_nurse_] I might be dehydrated.\n"
"The air is so hot and dry."
msgstr "[_nurse_] Potrei essere disidratato.\n"
"L'aria è così calda e asciutta."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:631 #-#-# translate crowdin quest_nurse_venting_walkie_talkie_55d6cbc3_1:
msgid "[_mc_] Do you need me to\n"
"come find you?"
msgstr "[_mc_] Hai bisogno di me per\n"
"venire a trovarti?"

