msgstr "[_mc_] Non devi più preoccuparti di\n"
"."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:761 #-#-# translate crowdin quest_nurse_venting_lost_04d0ddb9:
msgid "[_nurse_] How come?"
msgstr "[_nurse_] Come viene?"

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:762 #-#-# translate crowdin quest_nurse_venting_lost_a5cb96c0:
msgid "[_mc_] Well, let's just say she won't\n"
"be bothering you anymore."
msgstr "[_mc_] Beh, diciamo che non\n"
"darà più fastidio."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:763 #-#-# translate crowdin quest_nurse_venting_lost_56d7e57f_1:
msgid "[_nurse_] Oh..."
msgstr "[_nurse_] Oh..."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:764 #-#-# translate crowdin quest_nurse_venting_lost_088924ff:
msgid "[_mc_] You don't sound too\n"
