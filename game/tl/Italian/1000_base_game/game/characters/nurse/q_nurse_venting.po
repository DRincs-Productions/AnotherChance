# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:757 #-#-# translate crowdin quest_nurse_venting_lost_bec14fdf:
msgid "[_nurse_] I... I shouldn't say."
msgstr "[_nurse_] Io... non dovrei dire."

# game/1000_base_game/game/characters/nurse/q_nurse_venting.rpy:760 #-#-# translate crowdin quest_nurse_venting_lost_02a3c36b:
msgid "[_mc_] You don't have to worry\n"
