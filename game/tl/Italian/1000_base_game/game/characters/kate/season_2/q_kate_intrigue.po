# game/1000_base_game/game/characters/kate/season_2/q_kate_intrigue.rpy:47 #-#-# translate crowdin quest_kate_intrigue_start_8a826e82:
msgid "[kate] did say she'd have an airtight alibi, so maybe that's it?"
msgstr "[kate] ha detto che avrebbe avuto un alibi ermetico, quindi forse è vero?"

# game/1000_base_game/game/characters/kate/season_2/q_kate_intrigue.rpy:48 #-#-# translate crowdin quest_kate_intrigue_start_983c0f2c:
