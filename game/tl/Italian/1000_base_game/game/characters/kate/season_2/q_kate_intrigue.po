# game/1000_base_game/game/characters/kate/season_2/q_kate_intrigue.rpy:45 #-#-# translate crowdin quest_kate_intrigue_start_288e0257:
msgid "Unless there's something preventing legal action..."
msgstr "A meno che non ci sia qualcosa che impedisce l'azione legale..."

# game/1000_base_game/game/characters/kate/season_2/q_kate_intrigue.rpy:46 #-#-# translate crowdin quest_kate_intrigue_start_a20cefa7:
msgid "..."
