# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:260
msgid "\"You know, I've always found the way you talk to people repulsive.\""
msgstr "\"Sai, ho sempre trovato il modo in cui parli con le persone repulsive.\""

# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:273
msgid "\"Yes, [isabelle] was out of line.\""
msgstr "\"Sì, [isabelle] era fuori fila.\""

# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:273
msgid "\"No, you should leave her alone.\""
msgstr "\"No, la dovresti lasciare da sola.\""

# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:273
msgid "\"What's in it for me?\""
msgstr "\"Che cosa c'è per me?\""

# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:317
msgid "\"Okay, what's the plan?\""
