# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:533
msgid "\"Sorry, ma'am...\""
msgstr "\"Scusa, ma'am...\""

# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:533
msgid "\"You may be the queen of this school, but that doesn't mean I'll bow.\""
