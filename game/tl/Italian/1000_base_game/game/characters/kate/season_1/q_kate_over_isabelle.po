# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:351
msgid "\"I want to be left alone.\""
msgstr "\"Voglio essere lasciato da solo.\""

# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:351
msgid "\"I want to see her humiliated!\""
