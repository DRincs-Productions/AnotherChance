# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:87 #-#-# translate crowdin kate_quest_isabelle_tour_confront_side_isabelle_1cec36ce:
msgid "[_kate_s_confident_right_] He probably got tired of your voice... I know I did."
msgstr "[_kate_s_confident_right_] Probabilmente si è stancato della tua voce... So che l'ho fatto."

# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:88 #-#-# translate crowdin kate_quest_isabelle_tour_confront_side_isabelle_4d3bfec6:
