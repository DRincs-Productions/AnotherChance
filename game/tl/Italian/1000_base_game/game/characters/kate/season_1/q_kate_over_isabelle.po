# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:351
msgid "\"On second thought... fuck you.\""
msgstr "\"Al secondo pensiero... ti scopano.\""

# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:479
msgid "\"I'd hate to keep you waiting, ma'am.\""
msgstr "\"Odierei tenervi in attesa, ma'am.\""

# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:479
msgid "\"Do you have a reward for me?\""
msgstr "\"Hai una ricompensa per me?\""

# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:479
msgid "\"You better treat me well now.\""
msgstr "\"È meglio trattarmi bene adesso.\""

# game/1000_base_game/game/characters/kate/season_1/q_kate_over_isabelle.rpy:506
msgid "\"Yes, ma'am... sorry for assuming.\""
msgstr "\"Sì, ma'am... mi dispiace per aver presunto.\""

