# game/1000_base_game/game/characters/maxine/q_maxine_eggs.rpy:657 #-#-# translate crowdin quest_maxine_eggs_bathroom_encounter_c82ba496:
msgid "[_mc_] Right. I'll be going, then. Have a good night, [lindsey]!"
msgstr "[_mc_] Giusto. Andrò, allora. Buona notte, [lindsey]!"

