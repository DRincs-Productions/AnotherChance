msgid "{i}\"Do you have what it takes?\"{/}"
msgstr "{i}\"Hai quello che vuoi?\"{/}"

# game/1000_base_game/game/items/flora_poster.rpy:18 #-#-# translate crowdin flora_poster_interact_47633c16:
msgid "{i}\"We're looking for the best and brightest science students.\"{/}"
msgstr "{i}\"Stiamo cercando i migliori e più brillanti studenti di scienza.\"{/}"

# game/1000_base_game/game/items/flora_poster.rpy:19 #-#-# translate crowdin flora_poster_interact_01acbd8c:
msgid "{i}\"The Foundation offers a highly exciting work environment with new experiences and stimulating tasks.\"{/}"
msgstr "{i}\"La Fondazione offre un ambiente di lavoro molto emozionante con nuove esperienze e attività stimolanti.\"{/}"

# game/1000_base_game/game/items/flora_poster.rpy:20 #-#-# translate crowdin flora_poster_interact_c2a513f9:
msgid "{i}\"Welcome to the open house day at the Foundation!\"{/}"
msgstr "{i}\"Benvenuto nella giornata di apertura della casa presso la Fondazione!\"{/}"

# game/1000_base_game/game/items/flora_poster.rpy:21 #-#-# translate crowdin flora_poster_interact_703f1484:
msgid "{i}\"Hazmat suits and protective gear will be provided by us.\"{/}"
msgstr "{i}\"Le tute e gli attrezzi protettivi saranno forniti da noi.\"{/}"

