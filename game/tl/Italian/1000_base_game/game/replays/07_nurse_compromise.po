# game/1000_base_game/game/replays/07_nurse_compromise.rpy:43 #-#-# translate crowdin replay_nurse_compromise_53373587:
msgid "[_mc_] Your job and reputation mean everything to you, right?"
msgstr "[_mc_] Il tuo lavoro e la tua reputazione significano tutto per te, giusto?"

# game/1000_base_game/game/replays/07_nurse_compromise.rpy:44 #-#-# translate crowdin replay_nurse_compromise_e61f4628:
msgid "[_nurse_s_concerned_] Please, this can't get out!"
