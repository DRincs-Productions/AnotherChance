# game/1000_base_game/game/replays/07_nurse_compromise.rpy:37 #-#-# translate crowdin replay_nurse_compromise_00605032:
msgid "[_mc_] Just one for the yearbook, okay?"
msgstr "[_mc_] Solo uno per l'annuario, okay?"

# game/1000_base_game/game/replays/07_nurse_compromise.rpy:42 #-#-# translate crowdin replay_nurse_compromise_c0e2324d:
msgid "[_nurse_s_afraid_] W-what?"
