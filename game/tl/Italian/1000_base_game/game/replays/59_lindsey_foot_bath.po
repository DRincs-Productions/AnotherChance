# game/1000_base_game/game/replays/59_lindsey_foot_bath.rpy:118 #-#-# translate crowdin replay_lindsey_foot_bath_5a94ab23:
msgid "[_lindsey_] I... I don't know if I like this..."
msgstr "[_lindsey_] Io... Non so se mi piace questo..."

# game/1000_base_game/game/replays/59_lindsey_foot_bath.rpy:119 #-#-# translate crowdin replay_lindsey_foot_bath_80d46116:
msgid "[_kate_] It'll grow on you."
msgstr "[_kate_] Crescerà su di te."

# game/1000_base_game/game/replays/59_lindsey_foot_bath.rpy:120 #-#-# translate crowdin replay_lindsey_foot_bath_b19e9ee0:
msgid "The sole of her foot is coarse against my tongue."
