# TODO: Translation updated at 2023-11-05 22:06 #|#|# # game/1000_base_game/game/replays/62_nurse_pose.rpy:13 #-#-# translate crowdin replay_nurse_pose_de516b0b:
msgid "[_nurse_] B-but, Miss [kate]! They're right outside!"
msgstr "[_nurse_] B-but Signorina [kate]! Sono proprio fuori!"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:14 #-#-# translate crowdin replay_nurse_pose_079d43b4:
msgid "[_kate_] Yes, that's perfect for you. Someone could walk in here at any moment.{space=-65}"
msgstr "[_kate_] Sì, è perfetto per te. Qualcuno potrebbe entrare qui in qualsiasi momento.{space=-65}"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:15 #-#-# translate crowdin replay_nurse_pose_8c7d729b:
msgid "[_nurse_] Mhm..."
msgstr "[_nurse_] Mhm..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:16 #-#-# translate crowdin replay_nurse_pose_40194e30:
msgid "[_kate_] Make the round, slut. I will not repeat myself again."
msgstr "[_kate_] Fai il giro, la troia. Non mi ripeterò di nuovo."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:17 #-#-# translate crowdin replay_nurse_pose_b4eb6a6f:
msgid "[_nurse_] Yes, Miss [kate]..."
msgstr "[_nurse_] Sì, Signorina [kate]..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:25 #-#-# translate crowdin replay_nurse_pose_829b4e0f:
msgid "[_kate_] Come on, crawl."
msgstr "[_kate_] Avanti, striscia."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:26 #-#-# translate crowdin replay_nurse_pose_2dc06367:
msgid "[_kate_] If you're not quick, I'll make you eat that carrot afterward."
msgstr "[_kate_] Se non sei veloce, ti farò mangiare quella carota dopo."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:27 #-#-# translate crowdin replay_nurse_pose_9bd4e9c2:
msgid "Holy shit! The [nurse] is crawling after [kate] in broad daylight...\n"
"right at the party..."
msgstr "Merda santa! L' [nurse] sta strisciando dopo [kate] in pieno giorno...\n"
"proprio alla festa..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:29 #-#-# translate crowdin replay_nurse_pose_28f80cf5:
msgid "This is really hot..."
msgstr "Questo è davvero caldo..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:30 #-#-# translate crowdin replay_nurse_pose_f3ac8c8e:
msgid "But [kate] broke her promise."
msgstr "Ma [kate] ha rotto la sua promessa."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:31 #-#-# translate crowdin replay_nurse_pose_91d6350a:
msgid "[_mc_] What are you doing, [kate]?"
msgstr "[_mc_] Cosa stai facendo, [kate]?"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:32 #-#-# translate crowdin replay_nurse_pose_5c233d04:
msgid "[_nurse_s_kitchen_crawling_] Oh my goodness!"
msgstr "[_nurse_s_kitchen_crawling_] Oh la mia bontà!"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:33 #-#-# translate crowdin replay_nurse_pose_3aae6de2:
msgid "[_kate_] Look, we have company! It's my least favorite nerd!"
msgstr "[_kate_] Guarda, abbiamo compagnia! È il mio nerd meno preferito!"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:35 #-#-# translate crowdin replay_nurse_pose_b5c65360:
msgid "[_mc_] Err... I was just looking for ice cream..."
msgstr "[_mc_] Err... Stavo solo cercando del gelato..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:36 #-#-# translate crowdin replay_nurse_pose_2911ef33:
msgid "[_nurse_s_kitchen_crawling_] Oh, no... Oh, god..."
msgstr "[_nurse_s_kitchen_crawling_] Oh, no... Oh, dio..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:37 #-#-# translate crowdin replay_nurse_pose_4c75f1f7:
msgid "[_kate_] Please, I know you love this. And it looks like [mc] here brought\n"
"a camera!"
msgstr "[_kate_] Per favore, so che ti piace questo. E sembra che [mc] qui ha portato\n"
"una macchina fotografica!"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:39 #-#-# translate crowdin replay_nurse_pose_3cf123af:
msgid "[_mc_] You broke your promise to leave her alone."
msgstr "[_mc_] Hai rotto la tua promessa di lasciarla da sola."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:40 #-#-# translate crowdin replay_nurse_pose_2ae01185:
msgid "[_kate_] I didn't, actually. She asked me to do this."
msgstr "[_kate_] non l'ho fatto, in realtà. Mi ha chiesto di fare questo."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:41 #-#-# translate crowdin replay_nurse_pose_f932ac28:
msgid "[_kate_] Our deal was only to stop blackmailing her."
msgstr "[_kate_] Il nostro affare era solo per smettere di ricattarla."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:42 #-#-# translate crowdin replay_nurse_pose_2492d680:
msgid "[_mc_] I won't allow this, [kate]."
msgstr "[_mc_] Non consentirò questo, [kate]."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:43 #-#-# translate crowdin replay_nurse_pose_afb22d02:
msgid "[_kate_] Don't get your panties in a twist. This is the last time, okay?"
msgstr "[_kate_] Non ottenere le mutandine in una torsione. Questa è l'ultima volta, okay?"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:44 #-#-# translate crowdin replay_nurse_pose_d2484e0e:
msgid "[_kate_] She just wanted to say goodbye... the right way."
msgstr "[_kate_] Voleva solo dire addio... il modo giusto."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:45 #-#-# translate crowdin replay_nurse_pose_9646aee5:
msgid "[_mc_] Is this true, [nurse]?"
msgstr "[_mc_] È vero, [nurse]?"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:46 #-#-# translate crowdin replay_nurse_pose_02a337fc:
msgid "[_nurse_s_kitchen_crawling_] Y-yes..."
msgstr "[_nurse_s_kitchen_crawling_] Y-sì..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:47 #-#-# translate crowdin replay_nurse_pose_3a6c2e72:
msgid "[_kate_] See?"
msgstr "[_kate_] Vedi?"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:48 #-#-# translate crowdin replay_nurse_pose_479ea155:
msgid "[_kate_] Come on, slut! Pose for the camera!"
msgstr "[_kate_] Avanti, slut! Pose per la fotocamera!"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:49 #-#-# translate crowdin replay_nurse_pose_8d5d00c6:
msgid "[_nurse_s_kitchen_crawling_] My life is over..."
msgstr "[_nurse_s_kitchen_crawling_] La mia vita è finita..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:50 #-#-# translate crowdin replay_nurse_pose_d3246559:
msgid "[_kate_] Don't be such a drama queen."
msgstr "[_kate_] Non essere una regina così drammatica."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:51 #-#-# translate crowdin replay_nurse_pose_241ba01c:
msgid "[_kate_] This is only for the school paper. No one reads it."
msgstr "[_kate_] Questo è solo per la carta scolastica. Nessuno lo legge."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:59 #-#-# translate crowdin replay_nurse_pose_e6028fb3:
msgid "[_nurse_s_kitchen_posing_] Oh my god..."
msgstr "[_nurse_s_kitchen_posing_] Oh mio dio..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:60 #-#-# translate crowdin replay_nurse_pose_cbb5570a:
msgid "[_kate_] Take the picture, [mc]."
msgstr "[_kate_] Scatta la foto, [mc]."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:61 #-#-# translate crowdin replay_nurse_pose_dc6851f0:
msgid "[_mc_] I guess it'll look great on the cover..."
msgstr "[_mc_] Immagino che sarà grande sulla copertura..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:62 #-#-# translate crowdin replay_nurse_pose_521e06f0:
msgid "[_mc_] All right, smile!"
msgstr "[_mc_] Va bene, sorride!"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:63 #-#-# translate crowdin replay_nurse_pose_e6028fb3_1:
msgid "[_nurse_s_kitchen_posing_] Oh my god..."
msgstr "[_nurse_s_kitchen_posing_] Oh mio dio..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:71 #-#-# translate crowdin replay_nurse_pose_31995cf7:
msgid "Despite looking spooked, the [nurse] seems to have enjoyed that. She's practically gushing."
msgstr "Nonostante sembri spaventata, la [nurse] sembra essersi divertita. È praticamente spaventata."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:72 #-#-# translate crowdin replay_nurse_pose_b93997bf:
msgid "[_kate_] Keep crawling, slut. We're not done yet."
msgstr "[_kate_] Continua a strisciare, troia. Non abbiamo ancora finito."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:73 #-#-# translate crowdin replay_nurse_pose_3c28a2b9:
msgid "[_nurse_s_kitchen_posing_] Yes, Miss [kate]..."
msgstr "[_nurse_s_kitchen_posing_] Sì, Signorina [kate]..."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:74 #-#-# translate crowdin replay_nurse_pose_eb808d4b:
msgid "[_kate_] I'm starting to think you do want to eat that carrot."
msgstr "[_kate_] Sto iniziando a pensare che tu voglia mangiare quella carota."

# game/1000_base_game/game/replays/62_nurse_pose.rpy:75 #-#-# translate crowdin replay_nurse_pose_1fe5eecd:
msgid "[_kate_] Maybe I should make you eat it at the table?"
msgstr "[_kate_] Forse dovrei farti mangiare al tavolo?"

# game/1000_base_game/game/replays/62_nurse_pose.rpy:76 #-#-# translate crowdin replay_nurse_pose_98524035:
msgid "[_nurse_s_kitchen_posing_] Oh! Please, no!"
msgstr "[_nurse_s_kitchen_posing_] Oh! Per favore, no!"

