# game/1000_base_game/game/quests/season_1/kate_desire.rpy:256 #-#-# translate crowdin quest_kate_desire_nude_model_c867e6d2:
msgid "[_jacklyn_s_smile_] I wouldn't put my bank on that piggy if I were you."
msgstr "[_jacklyn_s_smile_] Non metterei la mia banca su quel porcellino se fossi in voi."

# game/1000_base_game/game/quests/season_1/kate_desire.rpy:257 #-#-# translate crowdin quest_kate_desire_nude_model_fd82ee84:
