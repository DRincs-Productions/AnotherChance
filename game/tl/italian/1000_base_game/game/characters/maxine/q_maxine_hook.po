# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:249 #-#-# translate crowdin quest_maxine_hook_lindsey_693058f4:
msgid "[_maxine_s_angry_] [lindsey]! You were supposed to drop off the watermelon in my office an hour ago!"
msgstr "[_maxine_s_angry_] [lindsey]! Avresti dovuto lasciare l'anguria nel mio ufficio un'ora fa!"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:250 #-#-# translate crowdin quest_maxine_hook_lindsey_177056b9:
msgid "[_lindsey_s_laughing_] Oh, right! I forgot about that... I've had things on my mind."
msgstr "[_lindsey_s_laughing_] Oh, giusto! Mi sono dimenticato... Ho avuto delle cose in mente."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:251 #-#-# translate crowdin quest_maxine_hook_lindsey_72b4e1a1:
msgid "[_maxine_s_neutral_] I'll need it soon. Please, don't forget it again."
