# TODO: Translation updated at 2023-11-05 22:06 #|#|# # game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:3 #-#-# translate crowdin quest_maxine_hook_start_8a0e7ef1:
msgid "[_mc_] Hey, [maxine]! What are you doing?"
msgstr "[_mc_] Hey, [maxine]! Cosa stai facendo?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:4 #-#-# translate crowdin quest_maxine_hook_start_316d7f6e:
msgid "[_maxine_s_concerned_binoculars_] I'm looking for the truth."
msgstr "[_maxine_s_concerned_binoculars_] Sto cercando la verità."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:5 #-#-# translate crowdin quest_maxine_hook_start_69b05675:
msgid "[_mc_] I know that, but..."
msgstr "[_mc_] lo so, ma..."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:6 #-#-# translate crowdin quest_maxine_hook_start_8d16ac39:
msgid "[_mc_] What are you up to right now?"
msgstr "[_mc_] Che cosa sei in questo momento?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:7 #-#-# translate crowdin quest_maxine_hook_start_19b99436:
msgid "[_maxine_s_thinking_] Talking to you, [mc]."
msgstr "[_maxine_s_thinking_] Parlando con te, [mc]."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:8 #-#-# translate crowdin quest_maxine_hook_start_f5d9423d:
msgid "[_maxine_s_thinking_] You really need to be more perceptive."
msgstr "[_maxine_s_thinking_] Hai davvero bisogno di essere più percettivo."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:9 #-#-# translate crowdin quest_maxine_hook_start_2b35b6fe:
msgid "[_mc_] ..."
msgstr "[_mc_] ..."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:10 #-#-# translate crowdin quest_maxine_hook_start_a77fe206:
msgid "[_mc_] What were you doing right before you started talking to me?"
msgstr "[_mc_] Cosa stavi facendo bene prima di iniziare a parlare con me?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:11 #-#-# translate crowdin quest_maxine_hook_start_a0e61930:
msgid "[_maxine_s_sad_] Taking care of business."
msgstr "[_maxine_s_sad_] Prendersi cura degli affari."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:12 #-#-# translate crowdin quest_maxine_hook_start_c956c812:
msgid "[_mc_] What business?"
msgstr "[_mc_] Che affari?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:13 #-#-# translate crowdin quest_maxine_hook_start_20abfdc5:
msgid "[_maxine_s_skeptical_] You ask an awful lot of questions."
msgstr "[_maxine_s_skeptical_] Fai un sacco di domande terribili."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:14 #-#-# translate crowdin quest_maxine_hook_start_67f87af8:
msgid "[_mc_] I'm just curious..."
msgstr "[_mc_] Sono semplicemente curioso..."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:15 #-#-# translate crowdin quest_maxine_hook_start_8e09ffd1:
msgid "[_maxine_s_eyeroll_] If you must know, I was looking for the narrative lens."
msgstr "[_maxine_s_eyeroll_] Se dovete sapere, stavo cercando la lente narrativa."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:16 #-#-# translate crowdin quest_maxine_hook_start_c8155816:
msgid "[_mc_] The what?"
msgstr "[_mc_] Il cosa?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:17 #-#-# translate crowdin quest_maxine_hook_start_d0d32b1b:
msgid "[_maxine_s_skeptical_] It's the window between worlds. Between fiction and reality."
msgstr "[_maxine_s_skeptical_] È la finestra tra i mondi. Tra finzione e realtà."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:18 #-#-# translate crowdin quest_maxine_hook_start_a4715e1f:
msgid "[_mc_] And what does this lens look like?"
msgstr "[_mc_] E che aspetto ha questo obiettivo?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:19 #-#-# translate crowdin quest_maxine_hook_start_488b4b3b:
msgid "[_maxine_s_skeptical_] It can be a camera, a canvas with an arrangement of colors, or even a person."
msgstr "[_maxine_s_skeptical_] Può essere una macchina fotografica, una tela con una disposizione di colori, o anche una persona."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:20 #-#-# translate crowdin quest_maxine_hook_start_be2994f7:
msgid "[_mc_] What? A person?"
msgstr "[_mc_] Cosa? Una persona?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:21 #-#-# translate crowdin quest_maxine_hook_start_e69b5e78:
msgid "[_maxine_s_smile_] The world is a stage, [mc] — everyone is a player. You know that, right?"
msgstr "[_maxine_s_smile_] Il mondo è un palco, [mc] — tutti sono un giocatore. Lo sapete, giusto?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:22 #-#-# translate crowdin quest_maxine_hook_start_0cb0ead9:
msgid "[_maxine_s_neutral_] But if everyone is on the stage, where is the audience?"
msgstr "[_maxine_s_neutral_] Ma se tutti sono sul palco, dov'è il pubblico?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:23 #-#-# translate crowdin quest_maxine_hook_start_2485f494:
msgid "[_mc_] I..."
msgstr "[_mc_] I..."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:24 #-#-# translate crowdin quest_maxine_hook_start_7fa92e55:
msgid "[_maxine_s_neutral_] Now, if you'll excuse me—"
msgstr "[_maxine_s_neutral_] Ora, se mi scuserai—"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:25 #-#-# translate crowdin quest_maxine_hook_start_8b5e620a:
msgid "[_mc_] Wait!"
msgstr "[_mc_] Aspetta!"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:26 #-#-# translate crowdin quest_maxine_hook_start_7e18bc27:
msgid "[_maxine_s_concerned_] What?"
msgstr "[_maxine_s_concerned_] Cosa?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:27 #-#-# translate crowdin quest_maxine_hook_start_e6856fd4:
msgid "[_mc_] I need your help with something."
msgstr "[_mc_] Ho bisogno del tuo aiuto con qualcosa."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:28 #-#-# translate crowdin quest_maxine_hook_start_4d59b575:
msgid "[_maxine_s_excited_] Well, what is it?"
msgstr "[_maxine_s_excited_] Beh, che cos'è?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:29 #-#-# translate crowdin quest_maxine_hook_start_f19754bf:
msgid "[_mc_] Do you believe in... {i}time travel{/}?"
msgstr "[_mc_] Credi in... {i}viaggio di tempo{/}?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:30 #-#-# translate crowdin quest_maxine_hook_start_7946bfa6:
msgid "[_maxine_s_thinking_] No."
msgstr "[_maxine_s_thinking_] No."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:31 #-#-# translate crowdin quest_maxine_hook_start_d9af6597:
msgid "[_mc_] No?"
msgstr "[_mc_] No?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:32 #-#-# translate crowdin quest_maxine_hook_start_13e81ec4:
msgid "[_mc_] But you believe in all sorts of wacky shit!"
msgstr "[_mc_] Ma tu credi in tutti i tipi di merda stravagante!"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:33 #-#-# translate crowdin quest_maxine_hook_start_8bc566b9:
msgid "[_maxine_s_thinking_] Time travel doesn't make any logical sense."
msgstr "[_maxine_s_thinking_] Il viaggio in tempo non ha alcun senso logico."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:34 #-#-# translate crowdin quest_maxine_hook_start_9e518749:
msgid "[_mc_] And a saucer-eyed doll that controls the magnetic field does?!"
msgstr "[_mc_] E una bambola che controlla il campo magnetico fa?!"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:35 #-#-# translate crowdin quest_maxine_hook_start_0b52e790:
msgid "[_maxine_s_smile_] Of course."
msgstr "[_maxine_s_smile_] Certo."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:36 #-#-# translate crowdin quest_maxine_hook_start_54c8bdeb:
msgid "[_maxine_s_smile_] You can travel forward in time by going fast enough — but it's only relative to the people who don't move at that speed."
msgstr "[_maxine_s_smile_] È possibile viaggiare in avanti in tempo andando abbastanza veloce — ma è solo relativo alle persone che non si muovono a quella velocità."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:37 #-#-# translate crowdin quest_maxine_hook_start_eb27c04c:
msgid "[_maxine_s_neutral_] Backwards in time is simply impossible."
msgstr "[_maxine_s_neutral_] Indietro nel tempo è semplicemente impossibile."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:38 #-#-# translate crowdin quest_maxine_hook_start_c430fa2c:
msgid "[_mc_] Well, you're wrong, because I did travel back in time."
msgstr "[_mc_] Beh, hai torto, perché ho viaggiato indietro nel tempo."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:39 #-#-# translate crowdin quest_maxine_hook_start_27731261:
msgid "[_maxine_s_laughing_] And you're choosing to go to school despite finishing it once already?"
msgstr "[_maxine_s_laughing_] E stai scegliendo di andare a scuola nonostante la finitura già una volta?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:40 #-#-# translate crowdin quest_maxine_hook_start_97eb424b:
msgid "Man, she does have a point. What the hell am I doing here?"
msgstr "L'uomo ha un punto: che diavolo sto facendo qui?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:41 #-#-# translate crowdin quest_maxine_hook_start_0e95dc08:
msgid "[_maxine_s_sad_] Unless..."
msgstr "[_maxine_s_sad_] Indipendente..."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:42 #-#-# translate crowdin quest_maxine_hook_start_851f4057:
msgid "[_mc_] Unless?"
msgstr "[_mc_] Indipendente?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:43 #-#-# translate crowdin quest_maxine_hook_start_c27ceeb9:
msgid "[_maxine_s_thinking_] Unless school was a large contributing factor to why your life turned out so poorly."
msgstr "[_maxine_s_thinking_] A meno che la scuola non fosse un grande fattore che contribuisce al perché la tua vita si sia rivelata così povera."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:44 #-#-# translate crowdin quest_maxine_hook_start_fd3a10cc:
msgid "[_maxine_s_thinking_] Then you would want a redo."
msgstr "[_maxine_s_thinking_] Allora vorresti un redo."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:45 #-#-# translate crowdin quest_maxine_hook_start_40bb7941:
msgid "[_mc_] How did you know that?"
msgstr "[_mc_] Come lo sapevate?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:46 #-#-# translate crowdin quest_maxine_hook_start_72e91524:
msgid "[_maxine_s_skeptical_] No offense, but you're not exactly someone with great prospects."
msgstr "[_maxine_s_skeptical_] Nessuna offesa, ma non sei esattamente qualcuno con grandi prospettive."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:47 #-#-# translate crowdin quest_maxine_hook_start_1263e952:
msgid "Ouch."
msgstr "Ouch."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:48 #-#-# translate crowdin quest_maxine_hook_start_45177610:
msgid "[_maxine_s_skeptical_] Anyway, I have a meeting with a plant. Adieu."
msgstr "[_maxine_s_skeptical_] Comunque, ho un incontro con una pianta. Adieu."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:54 #-#-# translate crowdin quest_maxine_hook_start_e25dcd99:
msgid "I feel like I've made a difference since I returned, but maybe I'm destined for failure regardless of my actions?"
msgstr "Mi sento come se avessi fatto la differenza da quando sono tornato, ma forse sono destinato al fallimento indipendentemente dalle mie azioni?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:55 #-#-# translate crowdin quest_maxine_hook_start_2462c791:
msgid "Ugh, talking to [maxine] always makes my brain tired. I should probably call it a day already..."
msgstr "Ugh, parlando con [maxine] rende sempre il mio cervello stanco. Probabilmente dovrei chiamarlo già un giorno..."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:60 #-#-# translate crowdin quest_maxine_hook_wake_home_bedroom_door_9cf8b97f:
msgid "I'll just pretend I didn't see a butt and a pair of legs sticking out from under my bed..."
msgstr "Farò solo finta che non ho visto un culo e un paio di gambe attaccare fuori da sotto il mio letto..."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:68 #-#-# translate crowdin quest_maxine_hook_wake_home_bedroom_spinach_6fecd3df:
msgid "[_maxine_s_blush_] [spinach], how are your captors treating you?"
msgstr "[_maxine_s_blush_] [spinach], come ti trattano i tuoi prigionieri?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:69 #-#-# translate crowdin quest_maxine_hook_wake_home_bedroom_spinach_147445fd:
msgid "[_spinach_s_closedeyes_] Meow?"
msgstr "[_spinach_s_closedeyes_] Meow?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:70 #-#-# translate crowdin quest_maxine_hook_wake_home_bedroom_spinach_af8199a0:
msgid "[_maxine_s_sad_s_flip_] You are a hostage. It would be unethical for me to pet you."
msgstr "[_maxine_s_sad_s_flip_] Tu sei un ostaggio. Sarebbe immorale per me prenderti in compagnia."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:72 #-#-# translate crowdin quest_maxine_hook_wake_home_bedroom_spinach_b3072f0d:
msgid "[_spinach_s_neutral_] Meow!"
msgstr "[_spinach_s_neutral_] Meow!"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:75 #-#-# translate crowdin quest_maxine_hook_wake_home_bedroom_spinach_e716f243:
msgid "[_maxine_s_thinking_s_flip_] Fine... a single petting, then I'll leave you to your fate."
msgstr "[_maxine_s_thinking_s_flip_] Fine... una singola pettinatura, poi ti lascerò al tuo destino."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:77 #-#-# translate crowdin quest_maxine_hook_wake_home_bedroom_spinach_63a531e5:
msgid "[_spinach_s_drugged_] Roar!"
msgstr "[_spinach_s_drugged_] Ruggito!"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:80 #-#-# translate crowdin quest_maxine_hook_wake_home_bedroom_spinach_d6468d11:
msgid "[_maxine_s_skeptical_] No, that is highly incorrect."
msgstr "[_maxine_s_skeptical_] No, è molto errato."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:81 #-#-# translate crowdin quest_maxine_hook_wake_home_bedroom_spinach_0d15e29b:
msgid "[_maxine_s_skeptical_] My sources tell me that [mc] doesn't have any ties to a terrorist organization other than the IRS."
msgstr "[_maxine_s_skeptical_] Le mie fonti mi dicono che [mc] non ha alcun legame con un'organizzazione terroristica diversa dall'IRS."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:88 #-#-# translate crowdin quest_maxine_hook_wake_9dc02746:
msgid "[_mc_] [maxine]...?"
msgstr "[_mc_] [maxine]...?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:89 #-#-# translate crowdin quest_maxine_hook_wake_2d5bb2df:
msgid "[_mc_] What are you doing under my bed?"
msgstr "[_mc_] Cosa stai facendo sotto il mio letto?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:90 #-#-# translate crowdin quest_maxine_hook_wake_4a516291:
msgid "[_maxine_] Investigating."
msgstr "[_maxine_] Indagine."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:91 #-#-# translate crowdin quest_maxine_hook_wake_b76a3433:
msgid "[_mc_] Investigating what?"
msgstr "[_mc_] Indagare cosa?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:92 #-#-# translate crowdin quest_maxine_hook_wake_f6a37073:
msgid "[_maxine_] Your claims of time travel, of course."
msgstr "[_maxine_] Le tue rivendicazioni di viaggi di tempo, ovviamente."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:93 #-#-# translate crowdin quest_maxine_hook_wake_698cedff:
msgid "[_maxine_] But there only seems to be a crusty old sock, two empty batteries, and what appears to be a spider's nest here."
msgstr "[_maxine_] Ma sembra che ci sia solo una vecchia calza croccante, due batterie vuote, e quello che sembra essere un nido di ragno qui."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:94 #-#-# translate crowdin quest_maxine_hook_wake_2a591919:
msgid "[_mc_] How did you even get in here?"
msgstr "[_mc_] Come sei entrato anche qui?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:96 #-#-# translate crowdin quest_maxine_hook_wake_2f448ad9:
msgid "[_maxine_s_excited_] The same way everyone else does."
msgstr "[_maxine_s_excited_] Lo stesso modo in cui tutti gli altri fanno."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:97 #-#-# translate crowdin quest_maxine_hook_wake_2b975562:
msgid "[_mc_] I'm... not talking about how you were born..."
msgstr "[_mc_] Io... non parlare di come sei nato..."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:98 #-#-# translate crowdin quest_maxine_hook_wake_b582e4df:
msgid "[_maxine_s_concerned_] Neither am I."
msgstr "[_maxine_s_concerned_] Né sono io."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:99 #-#-# translate crowdin quest_maxine_hook_wake_746598eb:
msgid "[_mc_] How did you get into my room, [maxine]?"
msgstr "[_mc_] Come sei entrato nella mia stanza, [maxine]?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:100 #-#-# translate crowdin quest_maxine_hook_wake_8217920f:
msgid "[_maxine_s_thinking_] Do not ask questions you don't want the answer to."
msgstr "[_maxine_s_thinking_] Non fare domande che non si desidera la risposta."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:101 #-#-# translate crowdin quest_maxine_hook_wake_2b35b6fe:
msgid "[_mc_] ..."
msgstr "[_mc_] ..."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:102 #-#-# translate crowdin quest_maxine_hook_wake_03226406:
msgid "[_maxine_s_thinking_] ..."
msgstr "[_maxine_s_thinking_] ..."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:103 #-#-# translate crowdin quest_maxine_hook_wake_ad39652d:
msgid "[_mc_] So... did you manage to draw any conclusions about my time traveling yet?"
msgstr "[_mc_] Quindi... sei riuscito a trarre qualche conclusione sul mio tempo che viaggia ancora?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:104 #-#-# translate crowdin quest_maxine_hook_wake_c813bb1d:
msgid "[_maxine_s_skeptical_] Yes, my conclusion is that you're lying."
msgstr "[_maxine_s_skeptical_] Sì, la mia conclusione è che stai mentendo."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:105 #-#-# translate crowdin quest_maxine_hook_wake_09f928ce:
msgid "[_maxine_s_annoyed_] I could've been investigating the ghost of Pond Park. Yet here I am, wasting my time."
msgstr "[_maxine_s_annoyed_] Avrei potuto indagare sul fantasma di Pond Park. Eppure qui sto perdendo tempo."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:108 #-#-# translate crowdin quest_maxine_hook_wake_11fdb519:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:111 #-#-# translate crowdin quest_maxine_hook_wake_ae105fa0:
msgid "[_mc_] I'm not lying!"
msgstr "[_mc_] non sto mentendo!"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:112 #-#-# translate crowdin quest_maxine_hook_wake_f1d68ace:
msgid "[_maxine_s_skeptical_] Everybody lies. Especially you."
msgstr "[_maxine_s_skeptical_] Tutti mentono. Soprattutto te."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:113 #-#-# translate crowdin quest_maxine_hook_wake_bbeeb44e:
msgid "[_mc_] What's that supposed to mean?"
msgstr "[_mc_] Che cosa dovrebbe significare?"

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:114 #-#-# translate crowdin quest_maxine_hook_wake_cecce463:
msgid "[_maxine_s_skeptical_] It means that falsehoods are perpetuated by all humans, but you in particular."
msgstr "[_maxine_s_skeptical_] Significa che le falsità sono perpetuate da tutti gli esseri umani, ma in particolare."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:115 #-#-# translate crowdin quest_maxine_hook_wake_f98ae06b:
msgid "[_mc_] Why me in particular?"
msgstr "[_mc_] Perché in particolare?"

