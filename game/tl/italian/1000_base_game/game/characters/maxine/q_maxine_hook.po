# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:253 #-#-# translate crowdin quest_maxine_hook_lindsey_459d9cb9:
msgid "[_lindsey_s_smile_] See you later, [mc]."
msgstr "[_lindsey_s_smile_] Ci vediamo più tardi, [mc]."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:256 #-#-# translate crowdin quest_maxine_hook_lindsey_a999f18e:
msgid "[_maxine_s_neutral_] I'm also going to need that power supply soon."
msgstr "[_maxine_s_neutral_] Ho anche bisogno di quell'alimentatore presto."

# game/1000_base_game/game/characters/maxine/q_maxine_hook.rpy:257 #-#-# translate crowdin quest_maxine_hook_lindsey_95eada0a:
msgid "[_mc_] I'm not getting it for you."
