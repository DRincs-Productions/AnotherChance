# game/1000_base_game/game/characters/isabelle/q_isabelle_dethroning.rpy:83
msgid "\"It looks scarier with your long hair.\""
msgstr "\"Sembra più spaventoso con i capelli lunghi.\""

# game/1000_base_game/game/characters/isabelle/q_isabelle_dethroning.rpy:83
msgid "\"I meant that there's no real difference.\""
msgstr "\"Intendevo che non c'è vera differenza.\""

# game/1000_base_game/game/characters/isabelle/q_isabelle_dethroning.rpy:281
msgid "\"You don't have to.\""
msgstr "\"Tu non devi fare.\""

# game/1000_base_game/game/characters/isabelle/q_isabelle_dethroning.rpy:281
msgid "\"It's less degrading for you.\""
msgstr "\"È meno degradante per te.\""

# game/1000_base_game/game/characters/isabelle/q_isabelle_dethroning.rpy:281
