# game/1000_base_game/game/locations/school/ground_floor_west/int_glass_dome.rpy:19 #-#-# translate crowdin school_ground_floor_west_glassdome_interact_541fe717:
msgid "{i}\"The specimen in area 12 is to remain unscathed.\"{/}"
msgstr "{i}\"Il campione nell'area 12 deve rimanere indisturbato\".{/}"

# game/1000_base_game/game/locations/school/ground_floor_west/int_glass_dome.rpy:20 #-#-# translate crowdin school_ground_floor_west_glassdome_interact_9548d4fa:
