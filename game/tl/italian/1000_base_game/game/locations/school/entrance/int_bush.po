# game/1000_base_game/game/locations/school/entrance/int_bush.rpy:149 #-#-# translate crowdin school_entrance_bush_quest_kate_search_for_nurse_1708810f:
msgid "[_mc_] We'll see when we get there."
msgstr "[_mc_] Vedremo quando arriveremo lì."

# game/1000_base_game/game/locations/school/entrance/int_bush.rpy:150 #-#-# translate crowdin school_entrance_bush_quest_kate_search_for_nurse_43af206c:
