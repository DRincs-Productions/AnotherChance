# game/1000_base_game/game/locations/home/bedroom/int_alarm.rpy:173
msgid "Smash it again!"
msgstr "Smash di nuovo!"

# game/1000_base_game/game/locations/home/bedroom/int_alarm.rpy:173
msgid "Let the new clock live... for now"
msgstr "Lascia vivere il nuovo orologio... per ora"

# game/1000_base_game/game/locations/home/bedroom/int_alarm.rpy:192
msgid "Smash it! Enough is enough!"
msgstr "Colpiscilo! Basta basta!"

# game/1000_base_game/game/locations/home/bedroom/int_alarm.rpy:192
msgid "Calm the storm, no one dies today"
msgstr "Calma la tempesta, nessuno muore oggi"

