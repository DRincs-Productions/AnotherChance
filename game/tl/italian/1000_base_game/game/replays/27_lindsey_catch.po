# TODO: Translation updated at 2023-11-05 22:06 #|#|# # game/1000_base_game/game/replays/27_lindsey_catch.rpy:10 #-#-# translate crowdin replay_lindsey_catch_3a5b2e50:
msgid "[_lindsey_s_lindseyfall_] Eeep!"
msgstr "[_lindsey_s_lindseyfall_] Eeep!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:19 #-#-# translate crowdin replay_lindsey_catch_44dd2dc6:
msgid "[_lindsey_s_lindseycatch_] Oof!"
msgstr "[_lindsey_s_lindseycatch_] Oof!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:20 #-#-# translate crowdin replay_lindsey_catch_5f25a052:
msgid "[_mc_] I got you."
msgstr "[_mc_] ti ho presa."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:21 #-#-# translate crowdin replay_lindsey_catch_72ecb4f5:
msgid "That was too close."
msgstr "Era troppo vicino."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:22 #-#-# translate crowdin replay_lindsey_catch_6b263823:
msgid "[_lindsey_s_lindseycatch_look_] [mc]? Where did you come from?"
msgstr "[_lindsey_s_lindseycatch_look_] [mc]? Da dove sei venuto?"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:24 #-#-# translate crowdin replay_lindsey_catch_11fdb519:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:26 #-#-# translate crowdin replay_lindsey_catch_7eb61261:
msgid "[_mc_] I've been here all along."
msgstr "[_mc_] Sono stato qui da sempre."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:27 #-#-# translate crowdin replay_lindsey_catch_048956fb:
msgid "[_mc_] Maybe if you slowed down you'd notice the people around you."
msgstr "[_mc_] Forse se avessi rallentato noterai le persone che ti circondano."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:28 #-#-# translate crowdin replay_lindsey_catch_e58da0a1:
msgid "[_lindsey_s_lindseycatch_look_] I'm late for practice... thanks for catching me, though!"
msgstr "[_lindsey_s_lindseycatch_look_] Sono in ritardo per la pratica... grazie per avermi catturato, però!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:29 #-#-# translate crowdin replay_lindsey_catch_550ac48d:
msgid "[_mc_] You're going to hurt yourself if you keep being careless."
msgstr "[_mc_] Ti farai del male se continuerai ad essere imprudente."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:30 #-#-# translate crowdin replay_lindsey_catch_cfb3abbd:
msgid "[_lindsey_s_lindseycatch_smile_] I slipped on the stairs, that's all! I'll watch my step in the future."
msgstr "[_lindsey_s_lindseycatch_smile_] sono scivolato sulle scale, tutto! Guarderò il mio passo in futuro."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:31 #-#-# translate crowdin replay_lindsey_catch_02a51b23:
msgid "[_mc_] This is the third time you've been in an accident this year."
msgstr "[_mc_] Questa è la terza volta che sei in un incidente quest'anno."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:32 #-#-# translate crowdin replay_lindsey_catch_5838d841:
msgid "[_mc_] Are you sure you're okay?"
msgstr "[_mc_] Sei sicuro che stai bene?"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:33 #-#-# translate crowdin replay_lindsey_catch_f05fb9cc:
msgid "[_lindsey_s_lindseycatch_smile_] Never been better!"
msgstr "[_lindsey_s_lindseycatch_smile_] Mai è stato migliore!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:35 #-#-# translate crowdin replay_lindsey_catch_39b966f9:
msgid "[_mc_] Where did {i}you{/} come from?"
msgstr "[_mc_] Da dove è venuto {i}tu{/}?"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:36 #-#-# translate crowdin replay_lindsey_catch_2269dfbb:
msgid "[_mc_] I was just walking along and you fell out of the sky."
msgstr "[_mc_] Io stavo solo camminando e tu caddi dal cielo."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:37 #-#-# translate crowdin replay_lindsey_catch_449d4f8f:
msgid "[_mc_] Are you a bird? A plane? Maybe an angel?"
msgstr "[_mc_] Sei un uccello? Un aereo? Forse un angelo?"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:38 #-#-# translate crowdin replay_lindsey_catch_48cd79fc:
msgid "[_lindsey_s_lindseycatch_smile_] Hey, smooth!"
msgstr "[_lindsey_s_lindseycatch_smile_] Ehi, liscio!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:39 #-#-# translate crowdin replay_lindsey_catch_c55efa98:
msgid "[_mc_] I thought you were a runner. What's with the flight maneuvers?"
msgstr "[_mc_] Ho pensato che tu fossi un corridore. Cosa c'è con le manovre di volo?"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:40 #-#-# translate crowdin replay_lindsey_catch_9c8e4b8d:
msgid "[_lindsey_s_lindseycatch_smile_] My lifelong dream is actually to become a pilot."
msgstr "[_lindsey_s_lindseycatch_smile_] Il mio sogno per tutta la vita è in realtà diventare un pilota."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:41 #-#-# translate crowdin replay_lindsey_catch_a31340dd:
msgid "[_mc_] Really?"
msgstr "[_mc_] Davvero?"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:42 #-#-# translate crowdin replay_lindsey_catch_20914ad3:
msgid "[_lindsey_s_lindseycatch_smile_] No, silly! I just slipped."
msgstr "[_lindsey_s_lindseycatch_smile_] No, sciocco! Ho appena scivolato."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:43 #-#-# translate crowdin replay_lindsey_catch_2e53b177:
msgid "[_lindsey_s_lindseycatch_smile_] Thanks for catching me."
msgstr "[_lindsey_s_lindseycatch_smile_] Grazie per avermi catturato."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:44 #-#-# translate crowdin replay_lindsey_catch_b2a4c840:
msgid "[_mc_] That's it? I just saved your life."
msgstr "[_mc_] È vero? Ho appena salvato la tua vita."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:45 #-#-# translate crowdin replay_lindsey_catch_deeb643c:
msgid "[_lindsey_s_lindseycatch_look_] ..."
msgstr "[_lindsey_s_lindseycatch_look_] ..."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:46 #-#-# translate crowdin replay_lindsey_catch_d5b2dee2:
msgid "[_mc_] I'm kidding. Glad you're okay."
msgstr "[_mc_] sto scherzando. Sono felice che tu stia bene."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:47 #-#-# translate crowdin replay_lindsey_catch_8e31f088:
msgid "[_lindsey_s_lindseycatch_smile_] I thought I'd eat it for sure."
msgstr "[_lindsey_s_lindseycatch_smile_] Pensavo di averlo mangiato di sicuro."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:48 #-#-# translate crowdin replay_lindsey_catch_f1655ff3:
msgid "[_mc_] Me too. I thought for sure that—"
msgstr "[_mc_] anche io. Ho pensato per certo quello—"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:50 #-#-# translate crowdin replay_lindsey_catch_94004a03:
msgid "Oh."
msgstr "Oh."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:51 #-#-# translate crowdin replay_lindsey_catch_778e0e52:
msgid "Okay, then..."
msgstr "Ok, allora..."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:52 #-#-# translate crowdin replay_lindsey_catch_a20cefa7:
msgid "..."
msgstr "..."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:53 #-#-# translate crowdin replay_lindsey_catch_1078743e:
msgid "God, she's so warm."
msgstr "Dio, è così calda."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:54 #-#-# translate crowdin replay_lindsey_catch_a24340f8:
msgid "I used to dream about hugging her, but even in my fantasy, she never hugged me back."
msgstr "Avevo sognato di abbracciarla, ma anche nella mia fantasia, non mi abbracciò mai."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:55 #-#-# translate crowdin replay_lindsey_catch_ed0d4a0e:
msgid "Never thought it would be so different from [jo]'s hugs."
msgstr "Non ha mai pensato che sarebbe stato così diverso dagli abbracci di [jo]."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:56 #-#-# translate crowdin replay_lindsey_catch_3b9c748d:
msgid "When she wraps her arms around me, it feels like she's putting everything she has into it."
msgstr "Quando lei avvolge le braccia intorno a me, si sente come lei sta mettendo tutto ciò che ha in esso."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:57 #-#-# translate crowdin replay_lindsey_catch_a72ca322:
msgid "Maybe that's unique for her, being a star athlete. Maybe she puts all her effort into everything she does."
msgstr "Forse è unico per lei, essendo un atleta stellare. Forse mette tutto il suo sforzo in tutto quello che fa."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:58 #-#-# translate crowdin replay_lindsey_catch_966c99a5:
msgid "[_lindsey_s_lindseycatch_hug_] Thanks so much for catching me!"
msgstr "[_lindsey_s_lindseycatch_hug_] Grazie mille per avermi catturato!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:59 #-#-# translate crowdin replay_lindsey_catch_6819c974:
msgid "So, this is what true appreciation feels like."
msgstr "Quindi, questo è ciò che si sente vero apprezzamento."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:60 #-#-# translate crowdin replay_lindsey_catch_9494ffb3:
msgid "Who would've thought it'd be so soft?"
msgstr "Chi avrebbe pensato che sarebbe stato così morbido?"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:62 #-#-# translate crowdin replay_lindsey_catch_25d86ca4:
msgid "[_mc_] We have to stop meeting like this."
msgstr "[_mc_] Dobbiamo smettere di incontrarci così."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:63 #-#-# translate crowdin replay_lindsey_catch_f148395b:
msgid "[_lindsey_s_lindseycatch_smile_] Oh, hello there!"
msgstr "[_lindsey_s_lindseycatch_smile_] Oh, ciao lì!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:64 #-#-# translate crowdin replay_lindsey_catch_a6b59f10:
msgid "[_lindsey_s_lindseycatch_smile_] I don't mind it all too much."
msgstr "[_lindsey_s_lindseycatch_smile_] non mi dispiace tutto troppo."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:65 #-#-# translate crowdin replay_lindsey_catch_526efde5:
msgid "[_mc_] You could seriously have hurt yourself!"
msgstr "[_mc_] Avresti potuto seriamente farti del male!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:66 #-#-# translate crowdin replay_lindsey_catch_0f17b964:
msgid "[_mc_] What if I wasn't around?"
msgstr "[_mc_] E se non fossi in giro?"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:67 #-#-# translate crowdin replay_lindsey_catch_f895c2b8:
msgid "[_lindsey_s_lindseycatch_look_] You're serious..."
msgstr "[_lindsey_s_lindseycatch_look_] Sei serio..."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:68 #-#-# translate crowdin replay_lindsey_catch_d26d6bd0:
msgid "[_mc_] Of course I'm serious!"
msgstr "[_mc_] Naturalmente sono serio!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:69 #-#-# translate crowdin replay_lindsey_catch_8c1682f2:
msgid "[_mc_] I can't stand the thought of something bad happening to you."
msgstr "[_mc_] Non riesco a sopportare il pensiero di qualcosa di brutto accadendo a voi."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:70 #-#-# translate crowdin replay_lindsey_catch_c101e007:
msgid "[_lindsey_s_lindseycatch_smile_] But I'm okay!"
msgstr "[_lindsey_s_lindseycatch_smile_] Ma sto bene!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:71 #-#-# translate crowdin replay_lindsey_catch_64196e23:
msgid "[_mc_] Yes, but—"
msgstr "[_mc_] Sì, ma—"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:72 #-#-# translate crowdin replay_lindsey_catch_0c2b5db2:
msgid "[_lindsey_s_lindseycatch_kiss_] Mwwah!"
msgstr "[_lindsey_s_lindseycatch_kiss_] Mwwah!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:73 #-#-# translate crowdin replay_lindsey_catch_a106bd56:
msgid "It's hard to argue with a pair of lips against your cheek."
msgstr "È difficile litigare con un paio di labbra contro la guancia."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:74 #-#-# translate crowdin replay_lindsey_catch_20fdb8c6:
msgid "Her carelessness is the main reason we're even talking."
msgstr "La sua negligenza è il motivo principale per cui stiamo anche parlando."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:75 #-#-# translate crowdin replay_lindsey_catch_7598f14b:
msgid "It has led us to this moment. So, maybe it's wrong of me to be upset with her."
msgstr "Ci ha portato a questo momento. Quindi, forse è sbagliato di me essere sconvolto con lei."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:76 #-#-# translate crowdin replay_lindsey_catch_bf3564a1:
msgid "But she's just so precious. And I don't say that just 'cause she's kissing my cheek."
msgstr "Ma è così preziosa. E non lo dico solo perché mi sta baciando la guancia."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:77 #-#-# translate crowdin replay_lindsey_catch_30509c93:
msgid "[_lindsey_s_lindseycatch_hug_] Thanks for catching me."
msgstr "[_lindsey_s_lindseycatch_hug_] Grazie per avermi catturato."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:78 #-#-# translate crowdin replay_lindsey_catch_14b608ff:
msgid "Okay, maybe I don't mind it that much."
msgstr "Ok, forse non mi dispiace così tanto."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:79 #-#-# translate crowdin replay_lindsey_catch_f3e4dd88:
msgid "Not when her hair carries the scent of spring — that mild breeze that heralds the summer."
msgstr "Non quando i suoi capelli portano il profumo di primavera — quella brezza mite che annuncia l'estate."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:80 #-#-# translate crowdin replay_lindsey_catch_b66acb0c:
msgid "And her arms around me whisk away the mental anguish from years of misery and self-doubt."
msgstr "E le sue braccia intorno a me frustano l'angoscia mentale da anni di miseria e di dubbio di sé."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:81 #-#-# translate crowdin replay_lindsey_catch_b67ba85b:
msgid "Not when her soft chest presses into me, and our hearts beat in tandem."
msgstr "Non quando il suo torace morbido preme su di me, e i nostri cuori battono in tandem."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:82 #-#-# translate crowdin replay_lindsey_catch_e923d793:
msgid "Not when she whispers words that only I can hear."
msgstr "Non quando sussurra parole che solo io posso sentire."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:83 #-#-# translate crowdin replay_lindsey_catch_d655da58:
msgid "[_lindsey_s_lindseycatch_hug_] My hero!"
msgstr "[_lindsey_s_lindseycatch_hug_] Mio eroe!"

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:84 #-#-# translate crowdin replay_lindsey_catch_ece7b9c3:
msgid "And she means it too. Not a trace of sarcasm in her voice."
msgstr "E lei lo vuol dire anche. Non una traccia di sarcasmo nella sua voce."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:85 #-#-# translate crowdin replay_lindsey_catch_ce3f23bc:
msgid "If only we could stay like this forever."
msgstr "Se solo potessimo rimanere in questo modo per sempre."

# game/1000_base_game/game/replays/27_lindsey_catch.rpy:86 #-#-# translate crowdin replay_lindsey_catch_60c0ad16:
msgid "If only..."
msgstr "Se solo..."

