msgid "[_mc_] I don't have anything to lose, but you do."
msgstr "[_mc_] Je n'ai rien à perdre, mais vous le faites."

# game/1000_base_game/game/locations/home/hall/int_door_flora.rpy:104 #-#-# translate crowdin home_hall_door_flora_jacklyn_statement_florasad_4e80afc9:
msgid "[_flora_s_thinking_] You're right."
msgstr "[_flora_s_thinking_] Vous avez raison."

# game/1000_base_game/game/locations/home/hall/int_door_flora.rpy:105 #-#-# translate crowdin home_hall_door_flora_jacklyn_statement_florasad_047d42bf:
