msgstr "[_flora_s_eyeroll_] C'est la chose la plus stupide que j'ai entendue toute la journée, et j'ai parlé\n"
"au [maxine]."

# game/1000_base_game/game/characters/jo/q_jo_day.rpy:72 #-#-# translate crowdin quest_jo_day_flora_2b8af14b:
msgid "[_mc_] Talking about what?"
msgstr "[_mc_] Parler de quoi ?"

# game/1000_base_game/game/characters/jo/q_jo_day.rpy:73 #-#-# translate crowdin quest_jo_day_flora_87f755df:
msgid "[_flora_s_annoyed_] None of your business!"
msgstr "[_flora_s_annoyed_] Aucune de vos affaires !"

# game/1000_base_game/game/characters/jo/q_jo_day.rpy:74 #-#-# translate crowdin quest_jo_day_flora_08bd576c:
