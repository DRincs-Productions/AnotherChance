# game/1000_base_game/game/characters/lindsey/season_1/q_lindsey_wrong.rpy:38 #-#-# translate crowdin quest_lindsey_wrong_follow_945bc6f5:
msgid "Ugh, my jokes never seem to hit home."
msgstr "Ugh, mes blagues ne semblent jamais frapper à la maison."

# game/1000_base_game/game/characters/lindsey/season_1/q_lindsey_wrong.rpy:39 #-#-# translate crowdin quest_lindsey_wrong_follow_86038a1e:
msgid "[_mc_] Sorry, I was just trying to be funny about earlier."
msgstr "[_mc_] Désolé, j'essayais juste d'être drôle."

# game/1000_base_game/game/characters/lindsey/season_1/q_lindsey_wrong.rpy:40 #-#-# translate crowdin quest_lindsey_wrong_follow_635aae20:
msgid "[_lindsey_s_thinking_] I'm so confused."
msgstr "[_lindsey_s_thinking_] Je suis tellement confus."

# game/1000_base_game/game/characters/lindsey/season_1/q_lindsey_wrong.rpy:41 #-#-# translate crowdin quest_lindsey_wrong_follow_8ebcf577:
