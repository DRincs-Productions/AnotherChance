# game/1000_base_game/game/characters/lindsey/season_1/q_lindsey_wrong.rpy:139 #-#-# translate crowdin school_ground_floor_stairs_quest_lindsey_wrong_fall_6b263823:
msgid "[_lindsey_s_lindseycatch_look_] [mc]? Where did you come from?"
msgstr "[_lindsey_s_lindseycatch_look_] [mc] ? D'où tu viens ?"

# game/1000_base_game/game/characters/lindsey/season_1/q_lindsey_wrong.rpy:141 #-#-# translate crowdin school_ground_floor_stairs_quest_lindsey_wrong_fall_11fdb519_1:
