# game/1000_base_game/game/characters/lindsey/season_1/q_lindsey_wrong.rpy:42 #-#-# translate crowdin quest_lindsey_wrong_follow_79265387:
msgid "[_lindsey_s_thinking_] Hmm... I guess I must've forgotten about it. When did that happen?"
msgstr "[_lindsey_s_thinking_] Hmm ... Je suppose que j'ai dû l'oublier. Quand est-ce arrivé ?"

# game/1000_base_game/game/characters/lindsey/season_1/q_lindsey_wrong.rpy:43 #-#-# translate crowdin quest_lindsey_wrong_follow_2b35b6fe:
