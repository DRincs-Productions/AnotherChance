# game/1000_base_game/game/characters/mrsl/q_the_key.rpy:6 #-#-# translate crowdin mrsl_quest_the_key_start_028e27ea:
msgid "[_mc_] Oh, right. I totally forgot about that."
msgstr "[_mc_] Oh, non. J'ai complètement oublié ça."

# game/1000_base_game/game/characters/mrsl/q_the_key.rpy:7 #-#-# translate crowdin mrsl_quest_the_key_start_ee015905:
