# game/1000_base_game/game/characters/flora/q_flora_bonsai.rpy:296 #-#-# translate crowdin quest_flora_bonsai_bed_404087ff:
msgid "[_flora_s_bed_s_content_] Aww."
msgstr "[_flora_s_bed_s_content_] Aww."

# game/1000_base_game/game/characters/flora/q_flora_bonsai.rpy:298 #-#-# translate crowdin quest_flora_bonsai_bed_f3886e17:
msgid "[_mc_] Besides, real fighting should be done using your fists."
