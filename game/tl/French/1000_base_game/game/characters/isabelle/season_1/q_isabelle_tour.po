msgid "Ridiculous, when you think about it. The nicer you are, the less they like you..."
msgstr "Ridicule, quand tu y penses. Plus vous êtes gentille, moins ils vous aiment ..."

# game/1000_base_game/game/characters/isabelle/season_1/q_isabelle_tour.rpy:744 #-#-# translate crowdin isabelle_quest_isabelle_tour_nurse_lets_go_immediately_87be41f2:
msgid "[_nurse_s_concerned_] Okay, [lindsey], come with me."
msgstr "[_nurse_s_concerned_] Ok, [lindsey], viens avec moi."

# game/1000_base_game/game/characters/isabelle/season_1/q_isabelle_tour.rpy:755 #-#-# translate crowdin isabelle_quest_isabelle_tour_nurse_lets_go_immediately_9c766747:
msgid "[_mc_] It's my fault she's hurt... fetching you was the least I could do."
msgstr "[_mc_] C'est ma faute qu'elle a du mal ... à récupérer, c'était le moins que je puisse faire."

# game/1000_base_game/game/characters/isabelle/season_1/q_isabelle_tour.rpy:757 #-#-# translate crowdin isabelle_quest_isabelle_tour_nurse_lets_go_immediately_e08479b5:
msgid "[_nurse_s_blush_] That's a good attitude to have."
