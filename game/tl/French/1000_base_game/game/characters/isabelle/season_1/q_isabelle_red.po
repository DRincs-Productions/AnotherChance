# game/1000_base_game/game/characters/isabelle/season_1/q_isabelle_red.rpy:64 #-#-# translate crowdin quest_isabelle_red_start_7719757d:
msgid "[_jacklyn_s_laughing_] When the doves turn black."
msgstr "[_jacklyn_s_laughing_] Lorsque les colombes deviennent noires."

# game/1000_base_game/game/characters/isabelle/season_1/q_isabelle_red.rpy:65 #-#-# translate crowdin quest_isabelle_red_start_8a340642:
