# game/1000_base_game/game/characters/isabelle/season_1/q_isabelle_red.rpy:45 #-#-# translate crowdin quest_isabelle_red_start_4d265c62:
msgid "[_jacklyn_s_laughing_] My great great aunt's name was Lora."
msgstr "[_jacklyn_s_laughing_] Le nom de ma grande tante était Lora."

# game/1000_base_game/game/characters/isabelle/season_1/q_isabelle_red.rpy:46 #-#-# translate crowdin quest_isabelle_red_start_c1475308:
