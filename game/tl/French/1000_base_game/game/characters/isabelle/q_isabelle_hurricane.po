# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:10 #-#-# translate crowdin quest_isabelle_hurricane_start_878890cf:
msgid "[_isabelle_s_skeptical_] That's right."
msgstr "[_isabelle_s_skeptical_] C'est vrai."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:11 #-#-# translate crowdin quest_isabelle_hurricane_start_ae8f27df:
msgid "[_mc_] Well, do you have a revenge plan yet?"
msgstr "[_mc_] Eh bien, avez-vous un plan de vengeance?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:12 #-#-# translate crowdin quest_isabelle_hurricane_start_8c09486f:
msgid "[_isabelle_s_concerned_] It's justice I'm after, not revenge."
msgstr "[_isabelle_s_concerned_] C'est la justice que je suis, pas la vengeance."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:13 #-#-# translate crowdin quest_isabelle_hurricane_start_d847a964:
msgid "[_mc_] What's the difference?"
msgstr "[_mc_] Quelle est la différence?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:14 #-#-# translate crowdin quest_isabelle_hurricane_start_2c4dd844:
msgid "[_isabelle_s_concerned_] One is to settle a score, the other is to uphold the law."
msgstr "[_isabelle_s_concerned_] L'un est de régler une note, l'autre est de faire respecter la loi."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:15 #-#-# translate crowdin quest_isabelle_hurricane_start_e0f1450c:
msgid "[_mc_] And you're the law?"
msgstr "[_mc_] Et vous êtes la loi?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:16 #-#-# translate crowdin quest_isabelle_hurricane_start_4c47b0d7:
msgid "[_isabelle_s_laughing_] Someone has to be in this place."
msgstr "[_isabelle_s_laughing_] Quelqu'un doit être dans cet endroit."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:17 #-#-# translate crowdin quest_isabelle_hurricane_start_d5ae2301:
msgid "[_mc_] Fair enough."
msgstr "[_mc_] Fair suffisante."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:18 #-#-# translate crowdin quest_isabelle_hurricane_start_f98840e6:
msgid "[_isabelle_s_confident_] And yes, I do in fact have a plan."
msgstr "[_isabelle_s_confident_] Et oui, j'ai en fait un plan."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:19 #-#-# translate crowdin quest_isabelle_hurricane_start_dc49e798:
msgid "[_mc_] We're not really killing [spinach], are we?"
msgstr "[_mc_] Nous ne tuons pas vraiment [spinach], sommes-nous?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:20 #-#-# translate crowdin quest_isabelle_hurricane_start_f60d748a:
msgid "[_isabelle_s_sad_] No, not really. I was angry."
msgstr "[_isabelle_s_sad_] Non, pas vraiment. J'étais en colère."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:21 #-#-# translate crowdin quest_isabelle_hurricane_start_00a930cd:
msgid "[_mc_] And you're not angry now?"
msgstr "[_mc_] Et vous n'êtes pas en colère maintenant?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:22 #-#-# translate crowdin quest_isabelle_hurricane_start_01922518:
msgid "[_isabelle_s_sad_] I'm not exactly happy, but I currently don't see red."
msgstr "[_isabelle_s_sad_] Je ne suis pas vraiment heureuse, mais je ne vois pas le rouge."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:25 #-#-# translate crowdin quest_isabelle_hurricane_start_11fdb519:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:28 #-#-# translate crowdin quest_isabelle_hurricane_start_7a95dcf1:
msgid "[_mc_] What color do you see now?"
msgstr "[_mc_] Quelle couleur voyez-vous maintenant?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:29 #-#-# translate crowdin quest_isabelle_hurricane_start_7f70ae00:
msgid "[_isabelle_s_confident_] I see a lighter shade of pink."
msgstr "[_isabelle_s_confident_] Je vois une teinte plus claire de rose."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:30 #-#-# translate crowdin quest_isabelle_hurricane_start_f21c8850:
msgid "[_mc_] The color of strawberry juice?"
msgstr "[_mc_] La couleur du jus de fraise?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:32 #-#-# translate crowdin quest_isabelle_hurricane_start_ab2b190e:
msgid "[_isabelle_s_laughing_] That's pretty cute."
msgstr "[_isabelle_s_laughing_] C'est mignon."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:35 #-#-# translate crowdin quest_isabelle_hurricane_start_4a132b61:
msgid "[_mc_] I also have a plan..."
msgstr "[_mc_] J'ai aussi un plan ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:36 #-#-# translate crowdin quest_isabelle_hurricane_start_47e23ea2:
msgid "[_isabelle_s_confident_] Surely, it has nothing to do with my knickers this time, right?"
msgstr "[_isabelle_s_confident_] Bien sûr, ça n'a rien à voir avec mes knickers cette fois, n'est-ce pas?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:37 #-#-# translate crowdin quest_isabelle_hurricane_start_b5699453:
msgid "[_mc_] Well, then I'm out of ideas."
msgstr "[_mc_] Eh bien, alors je suis en dehors des idées."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:38 #-#-# translate crowdin quest_isabelle_hurricane_start_77610fe7:
msgid "[_isabelle_s_laughing_] You're bad!"
msgstr "[_isabelle_s_laughing_] Vous êtes mauvais !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:39 #-#-# translate crowdin quest_isabelle_hurricane_start_08761323:
msgid "[_mc_] At literally everything I do. It's a talent."
msgstr "[_mc_] A littéralement tout ce que je fais. C'est un talent."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:40 #-#-# translate crowdin quest_isabelle_hurricane_start_7f1880f2:
msgid "[_isabelle_s_confident_] I know a few things you're good at."
msgstr "[_isabelle_s_confident_] Je sais quelques choses que vous êtes bonnes."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:41 #-#-# translate crowdin quest_isabelle_hurricane_start_42162a1e:
msgid "[_mc_] Do tell."
msgstr "[_mc_] Do tell."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:42 #-#-# translate crowdin quest_isabelle_hurricane_start_6119a72f:
msgid "[_isabelle_s_confident_] Making me laugh is one."
msgstr "[_isabelle_s_confident_] Je me moque de moi."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:43 #-#-# translate crowdin quest_isabelle_hurricane_start_8fba17ff:
msgid "[_mc_] And the others?"
msgstr "[_mc_] Et les autres?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:46 #-#-# translate crowdin quest_isabelle_hurricane_start_ad6484b8:
msgid "[_isabelle_s_blush_] Maybe I'll tell you later..."
msgstr "[_isabelle_s_blush_] Peut-être que je vous le dirai plus tard ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:47 #-#-# translate crowdin quest_isabelle_hurricane_start_bdf3cd67:
msgid "Why am I getting hard now? Are compliments a turn-on for me?"
msgstr "Pourquoi est-ce que j'ai du mal maintenant? Les compliments sont-ils un tour pour moi?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:48 #-#-# translate crowdin quest_isabelle_hurricane_start_9a64500b:
msgid "I better hurry things along so we get to the fabled {i}later!{/}"
msgstr "Je fais mieux de me dépêchez les choses, de sorte que nous nous sommes dirigés vers le fabuleux {i}plus tard ! { / }"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:52 #-#-# translate crowdin quest_isabelle_hurricane_start_679ad1ee:
msgid "[_mc_] I'm looking forward to punishing [maxine]."
msgstr "[_mc_] Je suis impatient de punir [maxine]."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:54 #-#-# translate crowdin quest_isabelle_hurricane_start_40ab0832:
msgid "[_isabelle_s_blush_] Same. We're going to teach her a proper lesson."
msgstr "[_isabelle_s_blush_] Même. Nous allons lui donner une bonne leçon."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:55 #-#-# translate crowdin quest_isabelle_hurricane_start_fc8f3ed5:
msgid "[_mc_] All right, so what's the plan?"
msgstr "[_mc_] Tout va bien, alors quel est le plan?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:56 #-#-# translate crowdin quest_isabelle_hurricane_start_214cc9ae:
msgid "[_isabelle_s_annoyed_left_] We first have to locate my locker."
msgstr "[_isabelle_s_annoyed_left_] Nous devons d'abord localiser mon casier."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:57 #-#-# translate crowdin quest_isabelle_hurricane_start_b6ca183d:
msgid "[_isabelle_s_annoyed_left_] It's not proper justice if the stolen belongings aren't returned to their rightful owners."
msgstr "[_isabelle_s_annoyed_left_] Ce n'est pas une justice appropriée si les biens volés ne sont pas rendus à leurs propriétaires légitimes."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:58 #-#-# translate crowdin quest_isabelle_hurricane_start_9e063cc6:
msgid "[_mc_] Then what?"
msgstr "[_mc_] Alors quoi?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:59 #-#-# translate crowdin quest_isabelle_hurricane_start_70035cdf:
msgid "[_isabelle_s_angry_] Well, she clearly doesn't care about things that are supposedly precious to her..."
msgstr "[_isabelle_s_angry_] Bien, elle ne se souciait clairement pas des choses qui sont supposément précieuses pour elle ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:60 #-#-# translate crowdin quest_isabelle_hurricane_start_09ccd3a1:
msgid "[_isabelle_s_angry_] So, retaliation has to come in a different form."
msgstr "[_isabelle_s_angry_] Alors, les représailles doivent se présenter sous une forme différente."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:61 #-#-# translate crowdin quest_isabelle_hurricane_start_362e38cd:
msgid "[_mc_] I thought you said this was about justice?"
msgstr "[_mc_] Je croyais que vous avez dit que c'était une question de justice?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:62 #-#-# translate crowdin quest_isabelle_hurricane_start_7709b98e:
msgid "[_isabelle_s_sad_] That's what I meant."
msgstr "[_isabelle_s_sad_] C'est ce que je voulais dire."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:63 #-#-# translate crowdin quest_isabelle_hurricane_start_08099467:
msgid "[isabelle] is clearly still on edge, and her anger could boil over at any moment..."
msgstr "[isabelle] est clairement toujours en avance, et sa colère pourrait se faire bouillir à tout moment ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:64 #-#-# translate crowdin quest_isabelle_hurricane_start_c497d73f:
msgid "[_mc_] Maybe it would be best if you let me take care of this?"
msgstr "[_mc_] Peut-être qu'il serait mieux que vous me laiez prendre soin de ça?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:65 #-#-# translate crowdin quest_isabelle_hurricane_start_f1cb1c18:
msgid "[_isabelle_s_concerned_] What makes you say that?"
msgstr "[_isabelle_s_concerned_] Qu'est-ce qui vous fait dire ça?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:66 #-#-# translate crowdin quest_isabelle_hurricane_start_34d3419c:
msgid "[_mc_] Having a cool head is pretty important when exacting one's revenge.{space=-10}"
msgstr "[_mc_] Avoir une tête froide est assez important lorsqu'il s'agit de se venger. { space=-10 }"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:67 #-#-# translate crowdin quest_isabelle_hurricane_start_93a7469e:
msgid "[_mc_] How about you just chill and leave this to me?"
msgstr "[_mc_] Comment vous détendre et laisser ça à moi?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:68 #-#-# translate crowdin quest_isabelle_hurricane_start_47a6e051:
msgid "[_isabelle_s_concerned_left_] That might be a good idea."
msgstr "[_isabelle_s_concerned_left_] C'est peut-être une bonne idée."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:69 #-#-# translate crowdin quest_isabelle_hurricane_start_7d8c58fa:
msgid "[_isabelle_s_skeptical_] But I want to know all the details after."
msgstr "[_isabelle_s_skeptical_] Mais je veux connaître tous les détails après."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:70 #-#-# translate crowdin quest_isabelle_hurricane_start_7c5c3d1a:
msgid "[_mc_] You can come over when I'm done and I'll tell you about it."
msgstr "[_mc_] Vous pouvez venir quand je suis fait et je vais vous en parler."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:71 #-#-# translate crowdin quest_isabelle_hurricane_start_4efb9563:
msgid "[_mc_] Maybe we can watch some NutFlex and order takeout?"
msgstr "[_mc_] Peut-être que nous pouvons regarder NutFlex et commander le décollage?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:72 #-#-# translate crowdin quest_isabelle_hurricane_start_c5d2bd3a:
msgid "[_isabelle_s_neutral_] Sounds good. I've been meaning to watch Quid Game forever."
msgstr "[_isabelle_s_neutral_] Sounds good. J'ai eu le sens de regarder Quid Game pour toujours."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:73 #-#-# translate crowdin quest_isabelle_hurricane_start_863f6cb2:
msgid "[_isabelle_s_neutral_] It was my sister's favorite survival show. Winner gets a million quid."
msgstr "[_isabelle_s_neutral_] C'était le spectacle de survie préféré de ma sœur. Le gagnant reçoit un million de quid."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:74 #-#-# translate crowdin quest_isabelle_hurricane_start_afb1d56c:
msgid "[_mc_] Sounds interesting! It's a date!"
msgstr "[_mc_] Les sons sont intéressants ! C'est une date !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:75 #-#-# translate crowdin quest_isabelle_hurricane_start_37480dd8:
msgid "[_isabelle_s_excited_] Okay!"
msgstr "[_isabelle_s_excited_] Ok !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:77 #-#-# translate crowdin quest_isabelle_hurricane_start_df79ec72:
msgid "All right, locating [isabelle]'s locker isn't a bad first step..."
msgstr "D'accord, localiser le casier de [isabelle]n'est pas une mauvaise première étape ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:78 #-#-# translate crowdin quest_isabelle_hurricane_start_c6d52c8c:
msgid "But where could [maxine] possibly be keeping it?"
msgstr "Mais où pourrait-on s'en tenir au [maxine]?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:83 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_0f3b1545:
msgid "Well, the locker sure as hell ain't here."
msgstr "Eh bien, le casier est sûr que l'enfer n'est pas là."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:84 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_722b01b3:
msgid "And I haven't seen it in any of the classrooms."
msgstr "Et je ne l'ai pas vu dans aucune des salles de classe."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:85 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_f78d75ca:
msgid "Where does one even store a stolen locker?"
msgstr "Où peut-on même stocker un casier volé?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:86 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_a20cefa7:
msgid "..."
msgstr "..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:91 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_155b33ec:
msgid "[_maxine_s_skeptical_] Why are you snooping around in my office?"
msgstr "[_maxine_s_skeptical_] Pourquoi vous saurez-vous dans mon bureau?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:94 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_11fdb519:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:97 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_440d5608:
msgid "[_mc_] I'm looking for a stolen locker."
msgstr "[_mc_] Je cherche un casier volé."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:98 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_725b8011:
msgid "[_maxine_s_concerned_] That's curious."
msgstr "[_maxine_s_concerned_] C'est curieux."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:99 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_5fa41194:
msgid "[_maxine_s_concerned_] A stolen locker? Why would anyone steal a locker?"
msgstr "[_maxine_s_concerned_] Un casier volé? Pourquoi quiconque volerait un casier?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:100 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_2b35b6fe:
msgid "[_mc_] ..."
msgstr "[_mc_]..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:101 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_54e6f545:
msgid "[_mc_] You're joking, right?"
msgstr "[_mc_] Vous plaisantez, n'est-ce pas?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:102 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_b506369a:
msgid "[_maxine_s_thinking_] I would never joke about larceny."
msgstr "[_maxine_s_thinking_] Je ne plaisanterais jamais sur le larcisme."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:103 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_c119869d:
msgid "[_mc_] But you stole it!"
msgstr "[_mc_] Mais vous l'avez volé !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:104 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_64e7c1e1:
msgid "[_maxine_s_thinking_] Do you have any proof of that?"
msgstr "[_maxine_s_thinking_] Avez-vous une preuve de cela?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:105 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_327961b8:
msgid "[_mc_] Well... you said it's been repurposed. So, you know... things."
msgstr "[_mc_] Eh bien ... vous avez dit que ça a été répuré. Donc, tu sais ... les choses."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:106 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_5706fbbe:
msgid "[_maxine_s_skeptical_] The last time I read through the statutory codes, knowledge was not listed as a crime."
msgstr "[_maxine_s_skeptical_] La dernière fois que j'ai lu les codes statutaires, les connaissances n'étaient pas énumérées comme un crime."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:107 #-#-# translate crowdin quest_isabelle_hurricane_maxine_office_7717d5c6:
