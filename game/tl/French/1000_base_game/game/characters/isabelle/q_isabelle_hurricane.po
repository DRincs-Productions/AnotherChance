# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:8 #-#-# translate crowdin quest_isabelle_hurricane_start_2ad88c74:
msgid "[_isabelle_s_skeptical_] ...and the cloud has a name."
msgstr "[_isabelle_s_skeptical_] ... et le nuage a un nom."

# game/1000_base_game/game/characters/isabelle/q_isabelle_hurricane.rpy:9 #-#-# translate crowdin quest_isabelle_hurricane_start_02e44be0:
msgid "[_mc_] [maxine]?"
