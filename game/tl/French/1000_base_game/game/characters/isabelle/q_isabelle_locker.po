# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:9 #-#-# translate crowdin quest_isabelle_locker_start_8ce03cbd:
msgid "[_isabelle_s_skeptical_] What do you think?"
msgstr "[_isabelle_s_skeptical_] Que pensez-vous?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:10 #-#-# translate crowdin quest_isabelle_locker_start_84f16542:
msgid "[_mc_] Okay..."
msgstr "[_mc_] Ok ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:11 #-#-# translate crowdin quest_isabelle_locker_start_2af48170:
msgid "[_isabelle_s_sad_] Sorry, I didn't mean to snap at you."
msgstr "[_isabelle_s_sad_] Désolé, je ne voulais pas vous impartir."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:12 #-#-# translate crowdin quest_isabelle_locker_start_0b15d407:
msgid "[_mc_] Don't worry about it."
msgstr "[_mc_] Ne vous inquiétez pas."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:13 #-#-# translate crowdin quest_isabelle_locker_start_0941a1d7:
msgid "[_isabelle_s_blush_] Really, you've been nothing but good to me throughout this!"
msgstr "[_isabelle_s_blush_] Vraiement, tu n'as été que bon pour moi tout au long de ça !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:14 #-#-# translate crowdin quest_isabelle_locker_start_a3ea86fd:
msgid "[_isabelle_s_sad_] Taking it out on you is simply unfair."
msgstr "[_isabelle_s_sad_] Si vous vous en avez fait, c'est tout simplement injuste."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:15 #-#-# translate crowdin quest_isabelle_locker_start_b43dbea3:
msgid "[_mc_] Hey, it's fine! Did you manage to get new books?"
msgstr "[_mc_] Hé, c'est bien ! Avez-vous réussi à obtenir de nouveaux livres?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:16 #-#-# translate crowdin quest_isabelle_locker_start_f119e3e8:
msgid "[_isabelle_s_neutral_] Yeah, I did. So, at least my grades won't suffer from this..."
msgstr "[_isabelle_s_neutral_] Oui, je l'ai fait. Donc, au moins mes notes ne vont pas souffrir de ça ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:17 #-#-# translate crowdin quest_isabelle_locker_start_e3e3c33d:
msgid "[_mc_] We'll get to the bottom of this. I promise."
msgstr "[_mc_] Nous irons au fond de cette situation. Je promets."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:18 #-#-# translate crowdin quest_isabelle_locker_start_04196621:
msgid "[_isabelle_s_concerned_] Thank you, [mc]."
msgstr "[_isabelle_s_concerned_] Merci, [mc]."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:19 #-#-# translate crowdin quest_isabelle_locker_start_3094ddd9:
msgid "[_isabelle_s_concerned_] Do you have a plan?"
msgstr "[_isabelle_s_concerned_] Avez-vous un plan?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:22 #-#-# translate crowdin quest_isabelle_locker_start_11fdb519:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:25 #-#-# translate crowdin quest_isabelle_locker_start_1eaafac2:
msgid "[_mc_] Nothing besides questioning as many people as possible."
msgstr "[_mc_] Rien d'autre que d'interroger autant de personnes que possible."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:26 #-#-# translate crowdin quest_isabelle_locker_start_a29c5694:
msgid "[_isabelle_s_confident_] It's as good a strategy as any."
msgstr "[_isabelle_s_confident_] C'est aussi une bonne stratégie que n'importe quel."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:27 #-#-# translate crowdin quest_isabelle_locker_start_7e4186b8:
msgid "[_mc_] Yeah, someone must know something..."
msgstr "[_mc_] Oui, quelqu'un doit savoir quelque chose ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:28 #-#-# translate crowdin quest_isabelle_locker_start_4921d3bc:
msgid "[_isabelle_s_confident_] I really appreciate you helping me out, [mc]."
msgstr "[_isabelle_s_confident_] J'apprécie vraiment que tu m'aimes, [mc]."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:29 #-#-# translate crowdin quest_isabelle_locker_start_685cf414:
msgid "[_mc_] You would've done the same for me."
msgstr "[_mc_] Vous auriez fait de même pour moi."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:30 #-#-# translate crowdin quest_isabelle_locker_start_0b9f251c:
msgid "[_isabelle_s_laughing_] Absolutely!"
msgstr "[_isabelle_s_laughing_] Absolument !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:33 #-#-# translate crowdin quest_isabelle_locker_start_4329129d:
msgid "[_mc_] Leave this in the hands of Sherlock!"
msgstr "[_mc_] Laissez ça entre les mains de Sherlock !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:34 #-#-# translate crowdin quest_isabelle_locker_start_ac5f843b:
msgid "[_isabelle_s_confident_] And that's supposed to be you?"
msgstr "[_isabelle_s_confident_] Et c'est censé être vous?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:35 #-#-# translate crowdin quest_isabelle_locker_start_3df01cd4:
msgid "[_mc_] This will be elementary, my dear [isabelle]."
msgstr "[_mc_] Ce sera élémentaire, mon cher [isabelle]."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:36 #-#-# translate crowdin quest_isabelle_locker_start_723a2d2e:
msgid "[_isabelle_s_confident_] You forgot to bring your pipe and deerstalker cap."
msgstr "[_isabelle_s_confident_] Vous avez oublié de porter votre calotte et votre pédonculer."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:37 #-#-# translate crowdin quest_isabelle_locker_start_1c2b00c9:
msgid "[_mc_] I also didn't bring my heroin. I'm Sherlock 2.0."
msgstr "[_mc_] I also didn't bring my héroïne. Je suis Sherlock 2.0."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:39 #-#-# translate crowdin quest_isabelle_locker_start_dc72de3c:
msgid "[_isabelle_s_laughing_] You've actually read it!"
msgstr "[_isabelle_s_laughing_] Vous l'avez lu !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:40 #-#-# translate crowdin quest_isabelle_locker_start_a5bbafc8:
msgid "[_mc_] Naturally."
msgstr "[_mc_] Naturellement."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:41 #-#-# translate crowdin quest_isabelle_locker_start_5e1cdfdf:
msgid "[_isabelle_s_flirty_] Okay, who do you think did this, Mr. Holmes?"
msgstr "[_isabelle_s_flirty_] Ok, qui a fait ça, M. Holmes?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:42 #-#-# translate crowdin quest_isabelle_locker_start_ff31f790:
msgid "[_mc_] It's a capital mistake to theorize before one has data..."
msgstr "[_mc_] C'est une erreur de capital à théoriser avant qu'on ait des données ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:43 #-#-# translate crowdin quest_isabelle_locker_start_c6140401:
msgid "[_isabelle_s_flirty_] A direct quote? Color me impressed!"
msgstr "[_isabelle_s_flirty_] Un devis direct? Couleur-moi impressionnée !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:44 #-#-# translate crowdin quest_isabelle_locker_start_a462ae1a:
msgid "[_mc_] You'll be even more impressed when I solve this case for you."
msgstr "[_mc_] Vous serez encore plus impressionné quand je vais résoudre ce cas pour vous."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:45 #-#-# translate crowdin quest_isabelle_locker_start_0210fa34:
msgid "[_mc_] I'll let you know when I have a lead."
msgstr "[_mc_] Je vous ferai savoir quand j'aurai une avance."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:46 #-#-# translate crowdin quest_isabelle_locker_start_29db47d7:
msgid "[_isabelle_s_blush_] Thanks so much for helping me out. I do appreciate it."
msgstr "[_isabelle_s_blush_] Merci beaucoup de m'avoir aidé. Je l'apprécie."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:48 #-#-# translate crowdin quest_isabelle_locker_start_273ff07b:
msgid "Let's see here. What would Sherlock have done?"
msgstr "Voyons voir ici. Qu'est-ce que Sherlock a fait?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:49 #-#-# translate crowdin quest_isabelle_locker_start_9c491626:
msgid "Gathered data... interrogations... yes, let's interrogate people."
msgstr "Des données dérangées ... des interrogatoires ... oui, interrogez les gens."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:52 #-#-# translate crowdin quest_isabelle_locker_start_3f03d602:
msgid "[_mc_] I might have an idea..."
msgstr "[_mc_] Je pourrais avoir une idée ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:53 #-#-# translate crowdin quest_isabelle_locker_start_8dc585e9:
msgid "[_isabelle_s_neutral_] Yeah?"
msgstr "[_isabelle_s_neutral_] Oui?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:54 #-#-# translate crowdin quest_isabelle_locker_start_bc8d36ad:
msgid "[_mc_] If you think about it, there's only one person in the school who deals in the bizarre."
msgstr "[_mc_] Si vous y pensez, il n'y a qu'une seule personne à l'école qui traite de l'étrange."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:55 #-#-# translate crowdin quest_isabelle_locker_start_d3a8624a:
msgid "[_mc_] And a missing locker certainly classifies as bizarre."
msgstr "[_mc_] Et un casier manquant classifie certainement comme bizarre."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:56 #-#-# translate crowdin quest_isabelle_locker_start_f788b34c:
msgid "[_isabelle_s_concerned_left_] You think [maxine] did this?"
msgstr "[_isabelle_s_concerned_left_] Vous pensez que [maxine] a fait ça?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:57 #-#-# translate crowdin quest_isabelle_locker_start_6068d40a:
msgid "[_mc_] Even if she didn't, she'll probably be interested in the story."
msgstr "[_mc_] Même si elle ne l'a pas fait, elle s'intéressera probablement à l'histoire."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:58 #-#-# translate crowdin quest_isabelle_locker_start_6dc04122:
msgid "[_mc_] Her newspaper is all about this sort of thing..."
msgstr "[_mc_] Son journal est à propos de ce genre de chose ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:59 #-#-# translate crowdin quest_isabelle_locker_start_91592038:
msgid "[_isabelle_s_excited_] That's smart! Let's go!"
msgstr "[_isabelle_s_excited_] C'est intelligent ! Allons-y !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:60 #-#-# translate crowdin quest_isabelle_locker_start_78f88725:
msgid "[_mc_] Wait. If she's the culprit, it might be best if I talk to her alone."
msgstr "[_mc_] Attente. Si c'est le coupable, c'est peut-être mieux que je parle à elle seule."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:61 #-#-# translate crowdin quest_isabelle_locker_start_960db493:
msgid "[_mc_] Your presence might give it away."
msgstr "[_mc_] Votre présence pourrait l'abandonner."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:62 #-#-# translate crowdin quest_isabelle_locker_start_1e4f0898:
msgid "[_isabelle_s_laughing_] I'm lucky to have made such a clever friend!"
msgstr "[_isabelle_s_laughing_] J'ai la chance d'avoir fait un si intelligent ami !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:63 #-#-# translate crowdin quest_isabelle_locker_start_98284d65:
msgid "[_mc_] Just trying to do my part. No need to get sentimental."
msgstr "[_mc_] Juste essayer de faire ma part. Pas besoin d'être sentimental."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:64 #-#-# translate crowdin quest_isabelle_locker_start_29446f55:
msgid "[_isabelle_s_confident_] Fine. Okay, I'll let you handle it."
msgstr "[_isabelle_s_confident_] Fine. Ok, je te laisse manipuler."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:73 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_cf7799fb:
msgid "[_flora_s_annoyed_] Why are you looking at me like I ripped your favorite anime poster?"
msgstr "[_flora_s_annoyed_] Pourquoi me regardez-vous comme si j'ai déchiré votre affiche préférée?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:74 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_9a24d05b:
msgid "[_mc_] Uh, that's not the look I was going for..."
msgstr "[_mc_] Uh, ce n'est pas le look que j'allais pour ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:75 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_f1ee3e28:
msgid "[_mc_] What about this?"
msgstr "[_mc_] Qu'en est-il de cela?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:76 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_2b35b6fe:
msgid "[_mc_] ..."
msgstr "[_mc_]..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:77 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_2613b4ef:
msgid "[_flora_s_annoyed_] ..."
msgstr "[_flora_s_annoyed_]..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:78 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_09ab3488:
msgid "[_flora_s_confused_] Now you're looking at me like I stole the last slice of your dried up pizza..."
msgstr "[_flora_s_confused_] Maintenant vous me regardez comme si j'ai volé la dernière tranche de votre pizza séchée ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:79 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_af9ac755:
msgid "[_mc_] Close enough, I guess."
msgstr "[_mc_] Fermer, je suppose."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:80 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_45c2cecc:
msgid "[_mc_] Did you see that someone took [isabelle]'s entire locker?"
msgstr "[_mc_] Avez-vous vu que quelqu'un a pris le casier entier de [isabelle]?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:81 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_ec267c53:
msgid "[_flora_s_laughing_] I did see that, yeah."
msgstr "[_flora_s_laughing_] J'ai vu ça, oui."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:82 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_d2dbdc80:
msgid "[_flora_s_blush_] Was it you?"
msgstr "[_flora_s_blush_] Était-ce vous?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:83 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_6a6197f1:
msgid "[_mc_] Me?! Why would you even think that?"
msgstr "[_mc_] Moi? Pourquoi tu penses même ça?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:84 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_8708f1fa:
msgid "[_flora_s_thinking_] Stealing a locker is weird. You're weird. Seems like a good suspect to me..."
msgstr "[_flora_s_thinking_] Le vol d'un casier est bizarre. Vous êtes bizarre. Ça ressemble à un bon suspect pour moi ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:85 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_0bb54b8f:
msgid "[_mc_] I'm not weird! Well... not stealing-a-locker-weird, at least."
msgstr "[_mc_] Je ne suis pas bizarre ! Eh bien ... pas voler-un casier-bizarre, au moins."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:86 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_2cd3ba80:
msgid "[_flora_s_annoyed_texting_] ..."
msgstr "[_flora_s_annoyed_texting_]..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:87 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_aca2b6de:
msgid "[_mc_] Who are you texting?"
msgstr "[_mc_] Qui es-tu textos?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:88 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_ee94a80a:
msgid "[_flora_s_annoyed_texting_] None of your business. Leave me alone."
msgstr "[_flora_s_annoyed_texting_] Aucune de vos activités. Laisse-moi tranquille."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:89 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_8e359492:
msgid "[_mc_] Fine."
msgstr "[_mc_] Fine."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:91 #-#-# translate crowdin quest_isabelle_locker_interrogate_flora_ba1c5a57:
msgid "Well, that didn't help much..."
msgstr "Eh bien, ça n'a pas beaucoup d'aide ..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:97 #-#-# translate crowdin quest_isabelle_locker_interrogate_jo_cdde83e0:
msgid "[_jo_s_confident_] Hey, kiddo. I was looking for you earlier."
msgstr "[_jo_s_confident_] Hey, kiddo. Je te cherchais plus tôt."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:98 #-#-# translate crowdin quest_isabelle_locker_interrogate_jo_f71fa49c:
msgid "[_mc_] Me? Why me?"
msgstr "[_mc_] Me? Pourquoi moi?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:99 #-#-# translate crowdin quest_isabelle_locker_interrogate_jo_ec55c909:
msgid "[_jo_s_neutral_] Because I wanted to ask you about the mess in the basement at home."
msgstr "[_jo_s_neutral_] Parce que je voulais vous poser la pagaille dans le sous-sol à la maison."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:100 #-#-# translate crowdin quest_isabelle_locker_interrogate_jo_dfe7c8cf:
msgid "[_mc_] I haven't been in the basement for a long time!"
msgstr "[_mc_] Je n'ai pas été dans le sous-sol depuis longtemps !"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:101 #-#-# translate crowdin quest_isabelle_locker_interrogate_jo_5686a214:
msgid "[_jo_s_smile_] I know, I know! But since you're all grown up now, I figured I should start asking you for more help at home."
msgstr "[_jo_s_smile_] Je sais, je sais ! Mais puisque vous êtes tous grandi maintenant, je me suis dit que je devrais commencer à vous demander plus d'aide à la maison."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:102 #-#-# translate crowdin quest_isabelle_locker_interrogate_jo_20ffd203:
msgid "[_jo_s_smile_] The basement needs a good cleaning."
msgstr "[_jo_s_smile_] Le sous-sol a besoin d'un bon nettoyage."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:105 #-#-# translate crowdin quest_isabelle_locker_interrogate_jo_11fdb519:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:108 #-#-# translate crowdin quest_isabelle_locker_interrogate_jo_6de1e072:
msgid "[_mc_] Can't [flora] do it?"
msgstr "[_mc_] Can't [flora] le faire?"

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:110 #-#-# translate crowdin quest_isabelle_locker_interrogate_jo_4a321e3d:
msgid "[_jo_s_eyeroll_] I asked you, [mc]..."
msgstr "[_jo_s_eyeroll_] Je vous ai demandé, [mc]..."

# game/1000_base_game/game/characters/isabelle/q_isabelle_locker.rpy:111 #-#-# translate crowdin quest_isabelle_locker_interrogate_jo_5d4b111b:
msgid "[_mc_] Well, I'm busy."
