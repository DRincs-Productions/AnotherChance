msgstr "[_lindsey_s_laughing_] Ha ! Bonne chance la prochaine fois !"

# game/1000_base_game/game/characters/maxine/q_maxine_eggs.rpy:569 #-#-# translate crowdin quest_maxine_eggs_bathroom_encounter_c34c970c:
msgid "[_lindsey_s_cringe_] That would've been creepy."
msgstr "[_lindsey_s_cringe_] Cela aurait été flirte."

# game/1000_base_game/game/characters/maxine/q_maxine_eggs.rpy:570 #-#-# translate crowdin quest_maxine_eggs_bathroom_encounter_a7be11ce:
