# game/1000_base_game/game/characters/maxine/q_maxine_eggs.rpy:553 #-#-# translate crowdin quest_maxine_eggs_bathroom_encounter_5d663b5b:
msgid "Where did [lindsey] even come from?"
msgstr "D'où vient le [lindsey]?"

# game/1000_base_game/game/characters/maxine/q_maxine_eggs.rpy:554 #-#-# translate crowdin quest_maxine_eggs_bathroom_encounter_4665819f:
msgid "[_mc_] Hey... sorry if I startled you."
msgstr "[_mc_] Hé ... désolé si je t'ai surpris."

# game/1000_base_game/game/characters/maxine/q_maxine_eggs.rpy:555 #-#-# translate crowdin quest_maxine_eggs_bathroom_encounter_065f3df4:
