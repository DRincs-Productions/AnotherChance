# game/1000_base_game/game/characters/maya/q_maya_spell.rpy:430 #-#-# translate crowdin quest_maya_spell_hair_isabelle_7fbd7791:
msgid "I wonder what else I could get away with? No time to find out now, but maybe later..."
msgstr "Je me demande ce que je pourrais faire d'autre ? Pas le temps de le découvrir maintenant, mais peut-être plus tard..."

# game/1000_base_game/game/characters/maya/q_maya_spell.rpy:431 #-#-# translate crowdin quest_maya_spell_hair_isabelle_f8b80985:
msgid "This must be how [kate] feels all the time."
