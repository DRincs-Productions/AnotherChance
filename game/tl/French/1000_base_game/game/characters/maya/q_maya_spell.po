msgstr "[_isabelle_s_sad_] Vous avez raison, c'est une bonne cause..."

# game/1000_base_game/game/characters/maya/q_maya_spell.rpy:444 #-#-# translate crowdin quest_maya_spell_hair_isabelle_b39e1ac7:
msgid "I can see the wheels spinning in her head."
