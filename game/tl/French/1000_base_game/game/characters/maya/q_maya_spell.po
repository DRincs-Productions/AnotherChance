# game/1000_base_game/game/characters/maya/q_maya_spell.rpy:419 #-#-# translate crowdin quest_maya_spell_hair_isabelle_1dcf964f:
msgid "[_mc_] Chippity, choppity. Give me a piece, else you'll end up with hair disease.{space=-70}"
msgstr "[_mc_] Chippity, choppity. Donnez-moi un morceau, sinon vous finirez par avoir une maladie des cheveux.{space=-70}"

# game/1000_base_game/game/characters/maya/q_maya_spell.rpy:420 #-#-# translate crowdin quest_maya_spell_hair_isabelle_a947d56a_1:
