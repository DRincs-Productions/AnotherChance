# game/1000_base_game/game/characters/maya/q_maya_spell.rpy:448 #-#-# translate crowdin quest_maya_spell_hair_isabelle_cd0a7509:
msgid "[_isabelle_s_subby_] All right, fine. I'll do it."
msgstr "[_isabelle_s_subby_] D'accord, très bien. Je vais le faire."

# game/1000_base_game/game/characters/maya/q_maya_spell.rpy:449 #-#-# translate crowdin quest_maya_spell_hair_isabelle_c5a9fd07:
