msgid "\"Help me get her to the [nurse]'s office.\""
msgstr "\"Aidez-moi à l'amener au bureau de [nurse].\""

# game/1000_base_game/game/replays/06_lindsey_collision.rpy:22
msgid "Carry [lindsey]"
msgstr "Report [lindsey]"

