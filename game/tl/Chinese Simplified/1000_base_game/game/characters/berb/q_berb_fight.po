# game/1000_base_game/game/characters/berb/q_berb_fight.rpy:136 #-#-# translate crowdin berb_quest_fight_roof_11fdb519_2:
msgid "[_extend_]  [nointeract]"
msgstr "[_extend_]  [nointeract]"

# game/1000_base_game/game/characters/berb/q_berb_fight.rpy:139 #-#-# translate crowdin berb_quest_fight_roof_11ea60a6:
msgid "[_mc_] He said all sorts of nasty things."
msgstr "[_mc_] 他说了各种无礼的话。"

# game/1000_base_game/game/characters/berb/q_berb_fight.rpy:140 #-#-# translate crowdin berb_quest_fight_roof_26d0a3e4:
msgid "[_flora_s_annoyed_] Like what?"
msgstr "[_flora_s_annoyed_] 比如呢？"

